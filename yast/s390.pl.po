msgstr "Akcja "

#: src/onpanic/ui.ycp:131
msgid "Delay &Minutes"
msgstr "Opóźnienie w minutach"

#. combobox label
#: src/onpanic/ui.ycp:136
msgid "&Dump Device"
msgstr "&Urządzenie zrzutu"

#: src/onpanic/ui.ycp:138
msgid "&VMCMD"
msgstr "VMCMD"

#: src/onpanic/ui.ycp:188
msgid "It is not possible to enable the dump process without a dump device."
msgstr "Nie można uruchomić procesu zrzutu bez urządzenia zrzutu."

#: src/onpanic/ui.ycp:196
msgid "It is not possible to use vmcmd  without defining at least one command."
msgstr "Nie można użyć vmcmd bez określenia co najmniej jednego polecenia."

