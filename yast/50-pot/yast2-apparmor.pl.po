"        <li>LD_PRELOAD</li>\n"
"        <li>LD_PROFILE</li>\n"
"        <li>LD_SHOW_AUXV</li>\n"
"        <li>LD_USE_LOAD_BIAS</li>\n"
"        <li>LOCALDOMAIN</li>\n"
"        <li>LOCPATH</li>\n"
"        <li>MALLOC_TRACE</li>\n"
"        <li>NLSPATH</li>\n"
"        <li>RESOLV_HOST_CONF</li>\n"
"        <li>RES_OPTION</li>\n"
"        <li>TMPDIR</li>\n"
"        <li>TZDIR</li> </ul>\n"
"\n"

