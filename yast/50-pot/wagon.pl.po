# mod
#: src/modules/Wagon.ycp:147
msgid ""
"\n"
"This module does not support command-line interface.\n"
"To do so, please, use zypper instead.\n"
msgstr ""
"\n"
"Ten moduł nie obsługuje interfejsu wiersza poleceń.\n"
"Proszę użyć zamiast niego programu zypper.\n"

#: src/modules/Wagon.ycp:153
msgid "Target file name ('xmlfile' option) is missing. Use xmlfile=<target_XML_file> command line option."
msgstr "Brak docelowej nazwy pliku ('xmlfile' opcja). Proszę użyć opcji polecenia:xmlfile=<docelowy_plik_XML>."

