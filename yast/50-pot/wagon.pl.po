#. heading text
#: src/clients/welcome_in_wagon.ycp:27
msgid "Welcome"
msgstr "Witamy"

#: src/clients/welcome_in_wagon.ycp:30
msgid "This tool will help to update the\nrunning system to a service pack.\n\nClick 'Next' to start the update."
msgstr "To narzędzie pomaga zaktualizować\nsystem do pakietu serwisowego.\n\nProszę nacisnąć klawisz \"Dalej\",\naby rozpocząć aktualizację."

#. help text
#: src/clients/welcome_in_wagon.ycp:37
msgid "<p>This tools updates the running system to a service pack.</p>"
msgstr "<p>To narzędzie aktualizuje system do pakietu serwisowego.</p>	"

#. TRANSLATORS: error message
#: src/modules/Wagon.ycp:84
msgid "No workflow defined."
msgstr "Nie zdefiniowano żadnego przepływu."

#: src/modules/Wagon.ycp:147
msgid "\nThis module does not support command-line interface.\nTo do so, please, use zypper instead.\n"
msgstr "\nTen moduł nie obsługuje interfejsu wiersza poleceń.\nProszę użyć zamiast niego programu zypper.\n"

#: src/modules/Wagon.ycp:153
msgid "Target file name ('xmlfile' option) is missing. Use xmlfile=<target_XML_file> command line option."
msgstr "Brak docelowej nazwy pliku (opcja \"xmlfile\"). Proszę użyć opcji polecenia: xmlfile=<docelowy_plik_XML>."

