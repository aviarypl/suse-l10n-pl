#: src/printingvianetwork.ycp:463 src/printingvianetwork.ycp:725
#, ycp-format
msgid "Failed to set 'ServerName %1' in /etc/cups/client.conf"
msgstr "Nie udało się ustawić dyrektywy \"ServerName %1\" pliku w /etc/cups/client.conf"

