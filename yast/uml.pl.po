msgid "Boot &Options"
msgstr "&Opcje rozruchowe"

#. help text for uml options
#: src/inst_uml_options.ycp:54
msgid "<P><B>Options</B></P>"
msgstr "<p><b>Opcje</b></p>"

#: src/inst_uml_options.ycp:55
msgid "<P>Set how much host system memory will be used by the virtual machine.</P>"
msgstr "<P>Należy ustalić, ile pamięci systemu głównego będzie używane przez komputer wirtualny.</P>"

#: src/inst_uml_options.ycp:56
msgid "<P>The UML machine can be started automatically on boot or manually when needed.</P>"
msgstr "<P>Komputer UML może być uruchamiany automatycznie w czasie uruchamiania systemu lub ręcznie w razie potrzeby.</P>"

#: src/inst_uml_options.ycp:57
#, ycp-format
msgid ""
"<P>Use the <I>%1</I> script for starting or stopping virtual machines or set\n"
"start mode <I>on boot</I> for automatic start.</P>\n"
msgstr ""
"<P>Do uruchamiania i zatrzymywania komputerów wirtualnych należy użyć \n"
"skryptu <I>%1</I>. W celu uruchamiania automatycznego należy ustawić \n"
"<I>Podczas uruchamiania systemu</I>.</P>\n"

#: src/inst_uml_options.ycp:60
msgid "<P><I>Boot Options</I> are used as extra arguments for the UML kernel.</P>"
msgstr "<P><I>Opcje rozruchowe</I> są używane jako dodatkowe parametry jądra UML.</P>"

#. error popup
#: src/inst_uml_options.ycp:92
msgid "Invalid amount of memory."
msgstr "Niewłaściwa wartość rozmiaru pamięci."

