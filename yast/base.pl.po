#: library/commandline/src/CommandLine.ycp:356
#, ycp-format
msgid "Invalid value for option '%1' -- expected '%2', received %3"
msgstr "Nieprawidłowa wartość opcji '%1' -- oczekiwano '%2', otrzymano %3"

