#: src/nfs_server.ycp:295
msgid "Add a directory to export"
msgstr "Dodaj katalog do wyeksportowania"

#: src/nfs_server.ycp:299
msgid "Delete a directory from export"
msgstr "Usuń eksportowany katalog"

#: src/nfs_server.ycp:303
msgid "Set the parameters for domain, security and enablev4"
msgstr "Ustawia parametry dla domeny, zabezpieczeń oraz enablev4"

#: src/nfs_server.ycp:309
msgid "Directory to export"
msgstr "Katalog do wyeksportowania"

#: src/nfs_server.ycp:313
msgid "Host wild card for setting the access to directory"
msgstr "Wzorzec nazwy komputera dla określenia dostępu do katalogu"

#. command line option help (do not transl. 'man exports')
#: src/nfs_server.ycp:318
msgid "Export options (see 'man exports')"
msgstr "Opcje eksportu (zobacz 'man exports')"

#: src/nfs_server.ycp:322
msgid "Domain specification for NFSv4 Idmapping, such as 'localdomain' or 'abc.com' etc."
msgstr "Określenie domeny dla mapowania id NFSv4, takiej jak 'localdomain' lub 'abc.com' etc."

#: src/nfs_server.ycp:326
msgid "Yes/No option for enabling/disabling support for NFSv4"
msgstr "Opcja Tak/Nie włączająca/wyłączająca wsparcie dla NFSv4"

#: src/nfs_server.ycp:330
msgid "Yes/No option for enabling/disabling secure NFS"
msgstr "Opcja Tak/Nie włączająca/wyłączająca bezpieczny NFS"

#: src/routines.ycp:246 src/routines.ycp:250
#, ycp-format
msgid ""
"%3 and %4 are both exported with the option fsid=0\n"
"for the same client '%1' (contained in '%2')"
msgstr ""
"%3 i %4 są razem eksportowane z opcją fsid=0\n"
"dla tego samego klienta '%1' (zawartego w '%2')"

#. selection box label
#: src/routines.ycp:351
msgid "Dire&ctories"
msgstr "&Katalogi"

#. error popup message
#: src/routines.ycp:368
#, ycp-format
msgid ""
"The wild card or options string is invalid.\n"
"It must be shorter than 70 characters and it\n"
"must not contain spaces.\n"
msgstr ""
"Nieprawidłowy wzorzec albo opcja.\n"
"Określenie musi być krótsze niż 70 znaków \n"
"i nie może zawierać odstępów (spacji).\n"

#. error popup message
#: src/routines.ycp:387
msgid ""
"Invalid option.\n"
"Only letters, digits, and the characters =/.,_- are allowed."
msgstr ""
"Nieprawidłowa opcja.\n"
"Dozwolone są tylko litery, cyfry i znaki =/.,_- ."

#. error popup message
#: src/routines.ycp:420
#, ycp-format
msgid "Unknown option: '%1'"
msgstr "Nieznana opcja: %1"

#. error popup message
#: src/routines.ycp:427
#, ycp-format
msgid "Invalid option: '%1'"
msgstr "Nieprawidłowa opcja '%1'"

#. %1 is a list of exported paths
#: src/routines.ycp:454
#, ycp-format
msgid ""
"There are unbalanced parentheses in export options\n"
"for %1.\n"
"Probably there is spurious whitespace in the configuration file."
msgstr ""
"W opcjach eksportu dla %1 wykryto niezamknięty nawias.\n"
"Prawdopodobnie przyczyną jest zbędna spacja w pliku konfiguracyjnym."

