#: src/clients/inst_finish.ycp:187
msgid "Finished."
msgstr "Zakończono."

#. Button to reject a license agreement
#: src/clients/inst_info.ycp:42
msgid "I Do &Not Agree"
msgstr "&Nie zgadzam się"

#. dialog caption
#: src/clients/inst_license.ycp:86
msgid "License Agreement"
msgstr "Umowa licencyjna"

#. radio button
#: src/clients/inst_license.ycp:102
msgid "Yes, I Agree to the License Agreement"
msgstr "Tak, zgadzam się z warunkami umowy licencyjnej"

#. radio button
#: src/clients/inst_license.ycp:106
msgid "No, I Do Not Agree"
msgstr "Nie, nie zgadzam się"

#. help text
#: src/clients/inst_license.ycp:115
msgid ""
"<p>Read the license agreement carefully and select\n"
"one of the available options. If you do not agree to the license agreement,\n"
"the configuration will be aborted.</p>\n"
msgstr ""
"<p>Proszę uważnie przeczytać warunki licencji i wybrać\n"
"jedną z dostępnych opcji. W przypadku niewyrażenia zgody\n"
"na warunki licencji konfiguracja zostanie przerwana.</p>\n"

#. yes-no popup
#: src/clients/inst_license.ycp:149
msgid "Reread or reconsider the license agreement?"
msgstr ""
"Aby kontynuować instalację, konieczne jest\n"
"zaakceptowanie umowy licencyjnej.\n"
"Czy wyświetlić umowę ponownie?"

#. timed ok/cancel popup
#: src/clients/inst_license.ycp:163
msgid "System is shutting down..."
msgstr "Zamykanie systemu..."

#. message popup
#: src/clients/inst_license.ycp:180
msgid "Accept or decline the license agreement."
msgstr "Proszę zaakceptować albo odrzucić warunki umowy licencyjnej."

#. label
#: src/clients/inst_mode.ycp:49
#: src/clients/inst_system_analysis.ycp:45
