#: src/passwd_nisplus.c:325
msgid "Could not determine hostname."
msgstr "Nie można ustalić nazwy komputera."

#: src/passwd_nisplus.c:341
msgid "RPC Secure Password: "
msgstr "Hasło Secure RPC: "

#: src/passwd_nisplus.c:344
#, c-format
msgid "Cannot find secret key for %s."
msgstr "Nie można znaleźć klucza prywatnego dla %s."

#: src/passwd_nisplus.c:349
#, c-format
msgid "Can't find %s's secret key"
msgstr "Nie można znaleźć klucza prywatnego %s"

#: src/passwd_nisplus.c:366 src/passwd_nisplus.c:389
#, c-format
msgid "Could not get public key for %s."
msgstr "Nie można uzyskać klucza publicznego dla %s."

#: src/passwd_nisplus.c:377
msgid "Could not determine the NIS+ root server."
msgstr "Nie można ustalić głównego serwera NIS+."

#: src/passwd_nisplus.c:398
msgid "Could not create conversion key."
msgstr "Nie można utworzyć klucza konwersji."

#: src/passwd_nisplus.c:412
msgid "DES encryption failure"
msgstr "Nieudane szyfrowanie DES"

#: src/passwd_nisplus.c:425
#, c-format
msgid "rpc.nispasswd not running on %s"
msgstr "rpc.nispasswd nie działa na %s"

#: src/passwd_nisplus.c:447
msgid "ERROR: Password incorrect. Try again"
msgstr "BŁĄD: Hasło nieprawidłowe. Proszę spróbować jeszcze raz."

#: src/passwd_nisplus.c:450
#, c-format
msgid ""
"ERROR: %s\n"
"       Password not changed"
msgstr ""
"BŁĄD: %s\n"
"       Hasło niezmienione"

#: src/passwd_nisplus.c:467 src/passwd_nisplus.c:488
msgid "DES decryption failure."
msgstr "Odszyfrowanie DES nie powiodło się."

#: src/passwd_nisplus.c:514
#, c-format
msgid ""
"ERROR: %s\n"
"       password not changed"
msgstr ""
"BŁĄD: %s\n"
"       hasło niezmienione"

#: src/passwd_nisplus.c:521
msgid "ERROR: Only partial success."
msgstr "BŁĄD: Sukces częściowy."

#: src/passwd_nisplus.c:529
#, c-format
msgid "\tPassword: %s"
msgstr "\tHasło: %s"

#: src/passwd_nisplus.c:534
#, c-format
msgid "\tgecos: %s"
msgstr "\tgecos: %s"

#: src/passwd_nisplus.c:538
#, c-format
msgid "\tShell: %s"
msgstr "\tPowłoka: %s"

#: src/passwd_nisplus.c:543
#, c-format
msgid "\tSecret Key: %s"
msgstr "\tKlucz prywatny: %s"

#: src/passwd_nisplus.c:553
msgid "ERROR: Unknown error."
msgstr "BŁĄD: Nieznany błąd."

#: src/passwd_nisplus.c:559
#, c-format
msgid ""
"NIS+ password information changed for %s\n"
"NIS+ credential information changed for %s"
msgstr ""
"Zmienione informacje passwd NIS+ dla %s\n"
"Zmienione informacje uwierzytelniające NIS+  dla %s"

#: src/passwd_nisplus.c:595
msgid "Password information update failed."
msgstr "Aktualizacja informacji passwd nie powiodła się."

#: src/passwd_nisplus.c:605
#, c-format
msgid "The NIS+ credential information for %s will not be changed."
msgstr "Informacje uwierzytelniające NIS+ dla %s nie będą zmienione."

#: src/passwd_nisplus.c:608
#, c-format
msgid "User %s must do the following to update credential information:"
msgstr "Aby zaktualizować informacje uwierzytelniające, użytkownik %s musi wykonać następujące czynności:"

#: src/passwd_nisplus.c:610
msgid "Use NEW password for login and OLD password for keylogin."
msgstr "Użyć NOWEGO hasła do zalogowania i STAREGO hasła w keylogin."

#: src/passwd_nisplus.c:612
msgid "Use \"chkey -p\" to reencrypt the credentials with the"
msgstr "Użyć \"chkey -p\" do ponownego zaszyfrowania uwierzytelnień z użyciem"

#: src/passwd_nisplus.c:613
msgid "new login passwd."
msgstr "nowego loginu passwd."

#: src/passwd_nisplus.c:615
msgid "The user must keylogin explicitly after next login."
msgstr "Użytkownik musi jawnie użyć keylogin po następnym logowaniu."

#: src/passwd_nisplus.c:640
#, c-format
msgid "NIS+ passwd table not found: %s"
msgstr "Tabela passwd NIS+ nieodnaleziona: %s"

#: src/passwd_nisplus.c:657
msgid "User not found in NIS+ table."
msgstr "Nie znaleziono użytkownika w tabeli NIS+."

#: src/support.c:130
msgid "Password: "
msgstr "Hasło: "

#: src/unix_acct.c:96 src/unix_acct.c:144
msgid "Your password has expired. Choose a new password."
msgstr "Hasło jest przeterminowane. Proszę podać nowe."

#: src/unix_acct.c:123
msgid "Password change requested. Choose a new password."
msgstr "Zażądano zmiany hasła. Proszę podać nowe."

#: src/unix_acct.c:307
#, c-format
msgid "Your password will expire in %ld days."
msgstr "Hasło wygaśnie za %ld dni."

#: src/unix_acct.c:311
msgid "Your password will expire tomorrow."
msgstr "Hasło wygaśnie jutro."

#: src/unix_acct.c:314
msgid "Your password will expire within 24 hours."
msgstr "Hasło wygaśnie w ciągu 24 godzin."

#: src/unix_auth.c:440
msgid "Permissions on the password database may be too restrictive."
msgstr "Uprawnienia dotyczące bazę haseł mogą być zbyt restrykcyjne."

#: src/unix_passwd.c:198
#, c-format
msgid "Less then %d weeks since the last change."
msgstr "Mniej niż %d tygodni od ostatniej zmiany."

#: src/unix_passwd.c:202
#, c-format
msgid "Less then %d days since the last change."
msgstr "Mniej niż %d dni od ostatniej zmiany."

#: src/unix_passwd.c:258
msgid "You can only change local passwords."
msgstr "Można zmieniać tylko lokalne hasła."

#: src/unix_passwd.c:338 src/unix_passwd.c:353
msgid "Password change aborted."
msgstr "Zmiana hasła przerwana."

#: src/unix_passwd.c:367
msgid "Passwords do not match."
msgstr "Hasła nie zgadzają się."

#: src/unix_passwd.c:392
msgid "Error: Password NOT changed."
msgstr "Błąd: Hasło NIE zostało zmienione."

#: src/unix_passwd.c:401
msgid "Password changed."
msgstr "Hasło zmienione."

#: src/unix_passwd.c:418
#, c-format
msgid "Cannot find the master ypserver: %s"
msgstr "Nie można znaleźć głównego serwera ypserver: %s"

#: src/unix_passwd.c:426
#, c-format
msgid "yppasswdd not running on NIS master %s"
msgstr "yppasswdd nie działa na głównym serwerze NIS %s"

#: src/unix_passwd.c:432
msgid "yppasswd daemon running on illegal port."
msgstr "Demon yppasswd działa na nielegalnym porcie."

#: src/unix_passwd.c:483
#, c-format
msgid "Cannot open %s for reading: %s"
msgstr "Nie można otworzyć pliku %s do odczytu: %s"

#: src/unix_passwd.c:492
#, c-format
msgid "Unable to obtain entropy from %s"
msgstr "Nie można uzyskać entropii z %s"

#: src/unix_passwd.c:507
msgid "Unable to generate a salt. Check your crypt settings."
msgstr "Nie można wygenerować wartości salt. Proszę sprawdzić ustawienia szyfrowania."

#: src/unix_passwd.c:641
msgid "Cannot create salt for standard crypt"
msgstr "Nie można utworzyć wartości salt. Proszę sprawdzić ustawienia szyfrowania."

#: src/unix_passwd.c:653
msgid "Cannot create salt for MD5 crypt"
msgstr "Nie można utworzyć wartości salt dla szyfrowania MD5"

#: src/unix_passwd.c:666
msgid "Cannot create salt for bigcrypt"
msgstr "Nie można utworzyć wartości salt dla bigcrypt"

#: src/unix_passwd.c:672
msgid "No support for bigcrypt included"
msgstr "Brak obsługi bigcrypt"

#: src/unix_passwd.c:684
msgid "Cannot create salt for blowfish crypt"
msgstr "Nie można utworzyć wartości salt dla szyfrowania blowfish"

#: src/unix_passwd.c:690
msgid "No support for blowfish included"
msgstr "Brak obsługi blowfish"

#: src/unix_passwd.c:702
msgid "crypt_r() returns NULL pointer"
msgstr "crypt_r() zwraca wskaźnik NULL"

#: src/unix_passwd.c:725
msgid "Cannot lock password file: already locked."
msgstr "Nie można zablokować pliku passwd: już jest zablokowany."

#: src/unix_passwd.c:752 src/unix_passwd.c:806 src/unix_passwd.c:886
#: src/unix_passwd.c:941
#, c-format
msgid "Cannot open %s: %m"
msgstr "Nie można otworzyć %s: %m"

#: src/unix_passwd.c:759 src/unix_passwd.c:893
#, c-format
msgid "Cannot stat %s: %m"
msgstr "Brak dostępu do do %s: %m"

#: src/unix_passwd.c:787 src/unix_passwd.c:921
#, c-format
msgid "Cannot create temp file (%s): %m"
msgstr "Nie można utworzyć pliku tymczasowego (%s): %m"

#: src/unix_passwd.c:836
#, c-format
msgid "Error while writing new shadow file: %m"
msgstr "Błąd podczas zapisu nowego pliku shadow: %m"

#: src/unix_passwd.c:846
#, c-format
msgid "Error while closing old shadow file: %m"
msgstr "Błąd podczas zamykania starego pliku shadow: %m"

#: src/unix_passwd.c:854
#, c-format
msgid "Error while closing temporary shadow file: %m"
msgstr "Błąd podczas zamykania tymczasowego pliku shadow: %m"

#: src/unix_passwd.c:967
#, c-format
msgid "Error while writing new password file: %m"
msgstr "Błąd podczas zapisu nowego pliku passwd: %m"

#: src/unix_passwd.c:977
#, c-format
msgid "Error while closing old password file: %m"
msgstr "Błąd podczas zamykania starego pliku passwd: %m"

#: src/unix_passwd.c:985
#, c-format
msgid "Error while closing temporary password file: %m"
msgstr "Błąd podczas zamykania tymczasowego pliku passwd: %m"

#: src/unix_passwd.c:1021
#, c-format
msgid "Changing NIS password for %s on %s."
msgstr "Zmiana hasła NIS dla %s na %s."

#: src/unix_passwd.c:1036
msgid "Error while changing the NIS password."
msgstr "Błąd podczas zmiany hasła NIS."

#: src/unix_passwd.c:1046
msgid "Error while changing the NIS+ password."
msgstr "Błąd podczas zmiany hasła NIS+."

