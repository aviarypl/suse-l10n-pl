#. VALID_SCHEMA
msgid "All required tables found. Schema is valid."
msgstr "Znaleziono wszystkie wymagane tabele. Schemat jest prawidłowy."

#. CHECKING_SCHEMA
msgid "Checking for database schema"
msgstr "Wyszukiwanie schematu dla bazy danych"

#. TABLE_FOUND
msgid "Checking schema for table {0}....found"
msgstr "Sprawdzanie schematu tabeli {0}.... znaleziono"

#. TABLE_NOT_FOUND
msgid "Checking schema for table {0}....missing"
msgstr "Wyszukiwanie schematu dla tabeli {0}.... nie znaleziono"

#. MISSING_MAPPING
msgid "Could not find Hibernate mapping for class {0}."
msgstr "Nie znaleziono mapowania Hibernate dla klasy {0}."

#. CREATING_SCHEMA
msgid "Creating database schema"
msgstr "Tworzenie schematu bazy danych"

#. UNICODE_TEST_ERROR
msgid "Database error verifying Unicode support, see stack trace for detail information."
msgstr "Wystąpił błąd bazy danych podczas sprawdzania obsługi Unicode; szczegółowe informacje - patrz śledzenie stosu."

#. ERR_ADD
msgid "Error adding object {0}"
msgstr "Błąd podczas dodawania obiektu {0}"

#. ERR_DELETE
msgid "Error deleting object {0}"
msgstr "Błąd podczas usuwania obiektu {0}"

#. ERR_FIND
msgid "Error finding object using example {0}"
msgstr "Błąd podczas wyszukiwania obiektu przy użyciu przykładu {0}"

#. ERR_ROLLBACK
msgid "Error rolling back transaction"
msgstr "Błąd podczas wycofywania transakcji"

#. DATA_FILE_PARSE
msgid "Failed to parse data load file {0}."
msgstr "Nie powiodło się przetwarzanie wczytanego pliku danych {0}."

#. SCHEMA_FILE_PARSE
msgid "Failed to parse database schema update file {0}."
msgstr "Nie powiodło się przetwarzanie pliku aktualizacji schematu bazy danych {0}."

#. MISSING_PRODUCER
msgid "Failed to retrieve producer id."
msgstr "Nie powiodło się pobranie ID producenta."

#. MISSING_SCHEMA_VER_INFO
msgid "No data found in table {0}."
msgstr "Nie znaleziono danych w tabeli {0}."

#. INVALID_SCHEMA
msgid "Schema is invalid. One or more required tables are missing. Check log for messages indicating tables that were not found."
msgstr "Schemat jest nieprawidłowy. Brakuje jednej lub więcej wymaganych tabeli. Proszę poszukać w dzienniku informacji o nieznalezionych tabelach."

#. UNICODE_NOT_SUPPORTED
msgid "The IDM User Application database does not support Unicode (UTF-8) encoded data. Unicode support is required for the IDM User Application. Please reconfigure the database with a Unicode-compatible character set."
msgstr "Baza danych aplikacji użytkownika IDM nie obsługuje danych kodowanych w Unicode (UTF-8). Obsługa Unicode jest wymagana dla aplikacji użytkownika IDM. Proszę przekonfigurować bazę danych z zestawem znaków kompatybilnym z Unicode."

#. UNSUPPORTED_UPGRADE
msgid "The database schema upgrade failed. No schema update information found for version {0}."
msgstr "Nie powiodła się aktualizacja schematu bazy danych. Nie znaleziono schematu aktualizacji dla wersji {0}."

#. INVALID_UPGRADE
msgid "The database schema upgrade to product type {0} from type {1} failed. No schema update information found for version {2}."
msgstr "Nie powiodła się aktualizacja schematu bazy danych do produktu typu {0} z produktu typu {1}. Nie znaleziono schematu aktualizacji dla wersji {2}."

#. NO_VERSION
msgid "Unable to determine current schema version from version XML file {0}."
msgstr "Nie można wyznaczyć wersji bieżącego schematu z pliku wersji XML {0}."

#. UNKNOWN_PRODUCT
msgid "Unknown product version, database schema check can\\'t proceed."
msgstr "Nieznana wersja produktu, nie można przetworzyć schematu bazy danych."

#. UNKNOWN_OPERATION
msgid "Unknown schema update operation {0}."
msgstr "Nieznana operacja aktualizacji schematu {0}."

#. UPDATING_SCHEMA_VERSION
msgid "Updating database schema to version {0}"
msgstr "Aktualizacja schematu bazy danych do wersji {0}"

#. UPDATING_SCHEMA
msgid "Upgrading database schema from {0} {1} to {2} {3}"
msgstr "Aktualizacja schematu bazy danych z {0} {1} do {2} {3}"

#. ARGUMENT_MISMATCH
msgid "Wrong number of arguments passed for SQL Update. Expected {0}, received {1}."
msgstr "Błędna liczba parametrów przekazana do polecenia SQL Update. Oczekiwano {0}, otrzymano {1}."
