#. DB_SCHEMA_DOES_NOT_EXISTS
msgid "Database schema for digital signature service is missing (was not populated) ."
msgstr "Brak schematu bazy danych dla podpisu cyfrowego (nie został wypełniony)."

#. ERROR
msgid "ERROR"
msgstr "BŁĄD"

#. TRANSACTION_ACCESS_PROBLEM
msgid "Failed to access Hibernate transaction."
msgstr "Nie powiodło się uzyskanie dostępu do transakcji hibernacji."

#. FAILED_TO_OPEN_TRANSACTION
msgid "Failed to begin Hibernate transaction."
msgstr "Nie powiodło się rozpoczęcie transakcji hibernacji."

#. DB_COMMIT_FAILURE
msgid "Failed to commit db transaction."
msgstr "Nie powiodło się przeprowadzenie transakcji db."

#. INIT_SERVICE_ERROR
msgid "Failed to initialize {0} service!"
msgstr "Nie powiodło się zainicjowanie usługi {0}."

#. INSTANCE_LOAD_ERROR
msgid "Failed to load instance of {0} class with id = {1}."
msgstr "Nie powiodło się wczytanie instancji klasy {0} o ID = {1}."

#. SRV_CFG_POPULATE_ERROR
msgid "Failed to populate initial data for {0} service."
msgstr "Nie powiodło się wczytanie danych początkowych dla usługi {0}."

#. DSS_INITIAL_DATA_POPULATE
msgid "Failed to populate persistent store of {0} service with initial data set!"
msgstr "Nie powiodło się wczytanie trwałego zasobu usługi {0} z zestawem danych początkowych."

#. INSTANCE_DELETE_ERROR
msgid "Failed to remove an instance of {0} class. Instance detail: {1}."
msgstr "Nie powiodło się usunięcie instancji klasy {0}. Szczegóły instancji: {1}."

#. DB_ROLLBACK_FAILURE
msgid "Failed to rollback database transaction."
msgstr "Nie powiodło się wycofanie transakcji bazy danych."

#. START_SERVICE_ERROR
msgid "Failed to start {0} service!"
msgstr "Nie powiodło się uruchomienie usługi {0}."

#. STOP_SERVICE_ERROR
msgid "Failed to stop {0} service!"
msgstr "Nie powiodło się zatrzymanie usługi {0}."

#. INITIALIZED
msgid "INITIALIZED"
msgstr "ZAINICJOWANA"

#. INVALID_STATE_RUNNING_INITIALIZED
msgid "Invalid service state: desired state is RUNNING or INITIALIZED!"
msgstr "Niewłaściwy stan usługi: oczekiwany to DZIAŁA albo ZAINICJOWANA."

#. INVALID_STATE_RUNNING
msgid "Invalid service state: desired state is RUNNING!"
msgstr "Niewłaściwy stan usługi: oczekiwany to DZIAŁA."

#. RUNNING
msgid "RUNNING"
msgstr "DZIAŁA"

#. SRV_UNSUPPORTED_OPERATION
msgid "Service {0} does not support {1} operation."
msgstr "Usługa {0} nie obsługuje operacji {0}."

#. UNINITIALIZED
msgid "UNINITIALIZED"
msgstr "NIEZAINICJOWANA"
