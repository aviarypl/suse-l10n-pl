msgid "Activity Id [{0}]: Error while forwarding to activity [{1}]."
msgstr "ID operacji [{0}]: błąd podczas przekazywania do operacji [{1}]."

#. ACTIVITY_ERROR_COMMON_MSG
msgid "Activity Id: [{0}]"
msgstr "ID operacji: [{0}]"

#. DUPLICATE_ENGINE_ID_ERROR
msgid "An engine with duplicate id: {0} and active heartbeat has been detected. Please restart this engine with a unique engine id."
msgstr "Wykryto silnik z powtarzającym się id: {0} oraz aktywny puls. Proszę uruchomić ten silnik ponownie z niepowtarzalnym id silnika."

#. APPROVED
msgid "Approved"
msgstr "Zaakceptowany"

#. DATA_TYPE_INCORRECT
msgid "Attempt to set value on Data item [{0}] using incorrect type; expecting [{1}] got [{2}].."
msgstr "Próba ustawienia wartości obiektu danych [{0}] z użyciem błędnego typu; oczekiwano [{1}], otrzymano [{2}].."

#. CANCELLED
msgid "Cancelled"
msgstr "Anulowany"

#. CLAIM_FAILURE
msgid "Claim operation failed for work entry [{0}]."
msgstr "Nie powiodła się operacja podjęcia dla wpisu zadania [{0}]."

