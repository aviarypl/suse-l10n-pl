msgid "Help: Create"
msgstr "Pomoc: tworzenie"

#. JSP_HELPPAGE_DEFAULT_CONTENT
msgid "\\u003ch1\\u003eCreate\\u003c/h1\\u003e \\u003cbr\\u003e \\u003cp\\u003eThis portlet enables you to create a new object, such as a user, group, or task group.\\u003c/p\\u003e \\u003cbr\\u003e \\u003cp\\u003e\\u003cb\\u003eFor more information\\u003c/b\\u003e, see the \\u003ca href=\\'http://www.novell.com/documentation/idm35/ugpro/data/ugproidentitycreate.html\\' target=\\'_blank\\'\\u003e documentation on creating users or groups\\u003c/a\\u003e in the \\u003ci\\u003eIdentity Manager User Application: User Guide\\u003c/i\\u003e on the Novell Web site.\\u003c/p\\u003e"
msgstr "\\u003ch1\\u003eCreate\\u003c/h1\\u003e \\u003cbr\\u003e \\u003cp\\u003eTen portlet pozwala na tworzenie nowego obiektu, jak użytkownik, grupa albo grupa zadań.\\u003c/p\\u003e \\u003cbr\\u003e \\u003cp\\u003e\\u003cb\\u003eWięcej informacji\\u003c/b\\u003e - patrz \\u003ca href=\\'http://www.novell.com/documentation/idm35/ugpro/data/ugproidentitycreate.html\\' target=\\'_blank\\'\\u003e dokumentacja dotycząca tworzenia użytkowników lub grup\\u003c/a\\u003e w \\u003ci\\u003ePodręczniku użytkownika aplikacji Identity Manager \\u003c/i\\u003e na stronie internetowej firmy Novell.\\u003c/p\\u003e"
