#. ERR_VIRTUAL_DATA
msgid "A virtual data error occurred"
msgstr "Wystąpił błąd danych wirtualnych"

#. ERR_UNKNOWN
msgid "An unknown error occurred."
msgstr "Wystąpił nieznany błąd."

#. style-res.alt-text-collapse
msgid "Collapse"
msgstr "Zwiń"

#. javax.portlet.title
msgid "Container Lookup"
msgstr "Wyszukiwanie kontenerów"

#. javax.portlet.description
#. javax.portlet.display-name
msgid "Container Lookup Portlet"
msgstr "Portlet wyszukiwania kontenerów"

#. javax.portlet.short-title
msgid "ContainerLookupPortlet"
msgstr "Portlet wyszukiwania kontenerów"

#. style-res.alt-text-expand
msgid "Expand"
msgstr "Rozwiń"

#. ERR_INVALID_ACTION
msgid "Invalid Action: no valid operation specified. Please contact your System Administrator."
msgstr "Nieprawidłowe dzialanie: nie wybrano prawidłowej operacji. Proszę skontaktować się z administratorem systemu."

#. javax.portlet.preference.name.max-rows-page
msgid "Max Rows Per Page"
msgstr "Maksymalna liczba wierszy na stronie"

#. ERR_EMPTY_ROOT_CONTAINER
msgid "No container objects were returned for the specified search root"
msgstr "W podanym zakresie wyszukiwania nie znaleziono żadnych obiektów kontenerów"

#. style-res.title
msgid "Root Directory:"
msgstr "Katalog główny:"

#. style-res.label-search
msgid "Search"
msgstr "Szukaj"

#. javax.portlet.preference.name.search-root
msgid "Search Root"
msgstr "Katalog główny wyszukiwania"

#. style-res.label-search-text
msgid "Search container list: (Example: A*, Lar*, ID, *r)"
msgstr "Lista wyszukiwanych kontenerów (przykład: A*, Lar*, ID, *r):"

#. style-res.label-container
msgid "Select a container from the list:"
msgstr "Proszę wybrać kontener z listy:"

#. style-res.msg-value-required
msgid "The attribute value is required for search."
msgstr "Do wyszukiwania wymagana jest wartość atrybutu."

#. ERR_INVALID_QUERY
msgid "The query is invalid."
msgstr "Nieprawidłowe zapytanie."

#. ERR_INVALID_DNLOOKUP_QUERY
msgid "The query must be built from an attribute that as a sub-type of DNLookup."
msgstr "Zapytanie musi być zbudowane z atrybutu o podtypie DNLookup."

#. ERR_INVALID_CONTAINER_QUERY
msgid "The query must be built from an entity that has a container entity definition."
msgstr "Zapytanie musi być zbudowane z jednostki posiadającej definicję kontenera jednostki."

#. ERR_INVALID_SESSION
msgid "The session is invalid."
msgstr "Nieprawidłowa sesja."

#. ERR_INVALID_DNLOOKUP_RENDER_PARM
msgid "This portlet must be passed a valid ENTITY_DEFINITION_KEY ({0}) and ATTRIBUTE_KEY ({1}). Make sure the calling portlet has set the necessary parameters."
msgstr "Do tego portletu muszą być przekazane prawidłowe wartości ENTITY_DEFINITION_KEY ({0}) oraz ATTRIBUTE_KEY ({1}). Proszę się upewnić, że wywołujący portlet ustawił wymagane parametry."

#. ERR_INVALID_RENDER_PARM
msgid "This portlet must be passed a valid portlet-unique-id ({0}), FORM_NAME ({1}), and FIELD_NAME ({2}). Make sure the calling portlet has set the necessary parameters."
msgstr "Do tego portletu muszą być przekazane: prawidłowy niepowtarzalny id portletu ({0}), FORM_NAME ({1}) i FIELD_NAME ({2}). Proszę się upewnić, że wywołujący portlet ustawił wymagane parametry."

#. style-res.submit-script-text
msgid "This script submits form data."
msgstr "Ten skrypt przesyła dane z formularza."

#. style-res.validate-script-text
msgid "This script validates data values, and passes the chosen value to the screen where the lookup was initiated."
msgstr "Ten skrypt sprawdza poprawność podanych wartości i przekazuje wybrane wartości do okna, w którym zostało zainicjowane wyszukiwanie."

#. javax.portlet.preference.name.max-rows-total
msgid "Total Rows Per Page"
msgstr "Całkowita liczba wierszy na stronę"

#. javax.portlet.keywords
msgid "list, param, DN lookup, Container lookup"
msgstr "lista, parametr, wyszukiwanie DN, wyszukiwanie kontenerów"


