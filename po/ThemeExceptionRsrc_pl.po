#. ERR_SECURITY_ADMIN_RIGHTS_REQUIRED
msgid "Administration rights are required to perform this operation."
msgstr "Do wykonania tej czynności wymagane są uprawnienia administratora."

#. ERR_LOCATING_FILE
msgid "An error was encountered locating the requested file \\'\\'{0}\\'\\'."
msgstr "Napotkano błąd podczas znajdowania żądanego pliku \"{0}\"."

#. ERR_LOCATING_THEME_DESCRIPTOR
msgid "An error was encountered locating the requested theme descriptor file \\'\\'{0}\\'\\'."
msgstr "Napotkano błąd podczas znajdowania żądanego pliku deskryptora motywu \"{0}\"."

#. ERR_RETRIEVING_BRAND
msgid "An error was encountered retrieving \\'\\'{0}\\'\\' brand."
msgstr "Napotkano błąd podczas pobierania marki \"{0}\"."

#. ERR_RETRIEVING_THEME
msgid "An error was encountered retrieving \\'\\'{0}\\'\\' theme."
msgstr "Napotkano błąd podczas pobierania motywu \"{0}\"."

#. ERR_RETRIEVING_THEME_LIST
msgid "An error was encountered retrieving the theme list."
msgstr "Napotkano błąd podczas pobierania listy motywów."

#. ERR_BRAND_ENTITY_CREATION_FAILURE
msgid "Brand Entity creation failure for \\'\\'{0}\\'\\'. Key \\'\\'{1}\\'\\'."
msgstr "Nie powiodło się tworzenie obiektu marki dla \"{0}\". Klucz \"{1}\"."

#. ERR_THEME_NAME_NOT_FOUND
msgid "Can\\'\\'t find \\'\\'{0}\\'\\' theme."
msgstr "Nie można znaleźć motywu \"{0}\"."

#. ERR_CLONE_EXCEPTION
msgid "Clone Exception."
msgstr "Wyjątek klonowania."

#. ERR_LOCKSMITH_CHECK
msgid "Error checking user for security rights. Error: {0}"
msgstr "Błąd podczas sprawdzania praw bezpieczeństwa użytkownika. Błąd: {0}"

#. ERR_EXCEPTION
msgid "Exception."
msgstr "Wyjątek."

#. ERR_IO_EXCEPTION
msgid "IO Exception."
msgstr "Wyjątek We/Wy."

#. ERR_METHOD_ARG_NOT_VALID
msgid "Illegal Argument value for \\'\\'{0}\\'\\'."
msgstr "Niedozwolona wartość parametru dla \"{0}\"."

#. ERR_METHOD_ARG_READONLY_BRAND_ISNOT_UPDATEABLE
msgid "Illegal Brand argument \\'\\'{0}\\'\\' since read-only objects are not updatable."
msgstr "Niedozwolony parametr marki \"{0}\", ponieważ nie można aktualizować obiektów tylko do odczytu."

#. ERR_BINARY64_DECODE
msgid "Internal failure performing Binary64 Decode. {0}"
msgstr "Wystąpił błąd wewnętrzny podczas dekodowania Binary64. {0}"

#. ERR_BINARY64_ENCODE
msgid "Internal failure performing Binary64 Encode. {0}"
msgstr "Wystąpił błąd wewnętrzny podczas kodowania Binary64. {0}"

#. ERR_INTERNAL_DIRECTORY_ACCESS_FAILURE
msgid "Internal failure trying to access the directory \\'\\'{0}\\'\\'. {1}"
msgstr "Wystąpił błąd wewnętrzny podczas próby uzyskania dostępu do katalogu \"{0}\". {1}"

#. ERR_INTERNAL_ACCESS_FAILURE
msgid "Internal failure trying to get access to \\'\\'{0}\\'\\'. {1}"
msgstr "Wystąpił błąd wewnętrzny podczas próby uzyskania dostępu do \"{0}\". {1}"

#. ERR_DEFAULT_THEME_NOT_FOUND_USING_DEFAULT
msgid "No persisted default theme was found. Will use internal default theme \\'\\'{0}\\'\\'."
msgstr "Nie znaleziono zapisanego domyślnego motywu. Zostanie użyty domyślny motyw wewnętrzny \"{0}\"."

#. ERR_RUNTIME_EXCEPTION
msgid "Runtime Exception."
msgstr "Wyjątek czasu działania."

#. ERR_RESOURCE_ARG_NOT_SUPPORTED
msgid "The Resource \\'\\'{0}\\'\\' does not support \\'\\'{1}\\'\\' calls."
msgstr "Zasób \"{0}\" nie obsługuje wywołań \"{1}\"."

#. ERR_DEFAULT_THEME_INVALID
msgid "The default theme cannot be determined."
msgstr "Nie można określić motywu domyślnego."

#. ERR_DEFAULT_THEME_USING_FIRST_FOUND
msgid "The default theme could not be determined, using first found in deployment: \\'\\'{0}\\'\\'"
msgstr "Nie można określić motywu domyślnego; zostanie użyty pierwszy motyw znaleziony w instalacji: \"{0}\""

#. ERR_DESCRIPTOR_INVALID
msgid "The descriptor (\\'\\'{0}\\'\\') for the \\'\\'{1}\\'\\' theme is not valid."
msgstr "Nieprawidłowy deskryptor (\"{0}\") dla motywu \"{1}\"."

#. ERR_XML_DOCUMENT_INVALID
msgid "The expected XML Document in file (\\'\\'{0}\\'\\') is not valid."
msgstr "Nieprawidłowy oczekiwany dokument XML w pliku (\"{0}\")."

#. ERR_OPERATION_NOT_SUPPORTED_IN_AUTO_MODE
msgid "The operation \\'\\'{0}\\'\\' is not supported on automatic theme mode."
msgstr "Czynność \"{0}\" nie jest obsługiwana dla automatycznego trybu motywu."

#. ERR_PERSISTED_DEFAULT_THEME_NOT_FOUND_USING_DEFAULT
msgid "The persisted default theme \\'\\'{0}\\'\\' was not found in the deployment directory. Will use internal default theme \\'\\'{1}\\'\\'."
msgstr "Zapisany domyślny motyw \"{0}\" nie został znaleziony w katalogu instalacji. Użyty zostanie domyślny motyw wewnętrzny \"{1}\"."

#. ERR_BRAND_TRANSFORMATION_FAILED
msgid "The transformation to generate the branding failed for \\'\\'{0}\\'\\' theme."
msgstr "Nie powiodła się transformacja generująca oprawę graficzną dla motywu \"{0}\"."

#. ERR_WRITE_OPERATION_NOT_SUPPORTED
msgid "The write operation is not supported on this read-only object."
msgstr "Operacja zapisu nie jest obsługiwana przez ten obiekt tylko do odczytu."
