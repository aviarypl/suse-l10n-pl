#. ERROR_CONSTRUCT_BEAN
msgid "An error occured while constructing bean: {0}"
msgstr "Wystąpił błąd podczas konstruowania obiektu bean: {0}"

#. ERR_SAVE_APPROVER_FAILED
msgid "An error occurred saving the Approver with dn: ({0}). Root cause: {1}"
msgstr "Wystąpił błąd podczas zapisywania akceptanta o DN: ({0}). Przyczyna: {1}"

#. INVALID_ACTION
msgid "An invalid action [{0}] has been requested. Please contact your System Administrator."
msgstr "Zażądano nieprawidłowej operacji [{0}]. Proszę skontaktować się z administratorem systemu."

#. FAILED_TO_SAVE_SOD
msgid "Error happened when trying to save SOD."
msgstr "Wystąpił błąd podczas próby zapisu RO."

#. FAILED_TO_RETRIEVE_SOD
msgid "Error occurred while trying to retrieve the SOD."
msgstr "Wystąpił błąd podczas próby pobrania RO."

#. FAILED_TO_SAVE_ROLE_CONFIG
msgid "Error occurred while trying to save the Roles Subsystem Configuration."
msgstr "Wystąpił błąd podczas próby zapisu konfiguracji podsystemu ról."

#. USER_BEAN_ERROR
msgid "Failed to find user authenticated information!"
