#. ERR_VALUE_REQUIRED
msgid "A value is required for {0}."
msgstr "Wymagana jest wartość dla {0}."

#. ERR_FIND_PACKED_ATTRIBUTES
msgid "Error finding Packed Attributes"
msgstr "Wystąpił błąd podczas znajdywania spakowanych atrybutów"

#. ERR_NOT_ALLOWED_CHAR
msgid "Some characters are not allowed due to XSS protection."
msgstr "Niektóre znaki nie są dozwolone z powodu ochrony przed XSS."

#. ERR_EMAIL_CANT_BE_VALIDATED
msgid "The email cannot be validated."
msgstr "Nie można zweryfikować tego adresu e-mail."

#. ERR_INVALID_EMAIL
msgid "The format for the email entry ({0}) is invalid."
msgstr "Błędy format dla wpisu e-mail ({0})."

#. ERR_OUT_OF_RANGE
msgid "The value for {0} must fall within the range of {1} and {2}."
msgstr "Wartość dla {0} musi zawierać się w zakresie od {1} do {2}."

#. ERR_INVALID_LIST_VALUE
msgid "{0} is not a valid list value. Please select a valid value, or contact your System Administrator."
msgstr "{0} nie jest prawidłową wartością listy. Proszę wybrać prawidłową wartość lub skontaktować się z administratorem systemu."

#. ERR_INVALID_INTEGER
msgid "{0} must be an integer."
msgstr "{0} musi być liczbą całkowitą."

