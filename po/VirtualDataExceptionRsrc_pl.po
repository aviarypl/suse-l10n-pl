#. ERR_FORMAT
msgid "Attribute [{0}] with value [{2}] at index [{1}] could not be converted to desired type"
msgstr "Atrybut [{0}] o wartości [{2}] o indeksie [{1}] nie może być przekonwertowany na żądany typ"

#. ERR_ATTRIBUTE_NULL
msgid "Attribute: [{0}] not found."
msgstr "Atrybut: [{0}] nie znaleziony."

#. ERR_ID_NOT_FOUND
msgid "Cannot find {0} with ID = \\'\\'{1}\\'\\'."
msgstr "Nie można znaleźć {0} o ID = \\'\\'{1}\\'\\'."

#. ERR_VDM_USERPREFS_RETRIEVE
msgid "Cannot retrieve user preferences. They may be ill-formed. Please contact your System Administrator."
msgstr "Nie można pobrać preferencji użytkownika. Mogą być one w złym formacie. Proszę skontaktować się z administratorem systemu."

#. ERR_VDM_QUERYLIST_SAVE
msgid "Cannot save updated query list. Please contact your System Administrator."
msgstr "Nie można zapisać zaktualizowanej listy zapytań. Proszę skontaktować się z administratorem systemu."

#. ERR_VDM_USERPREFS_SAVE
msgid "Cannot save user preferences. Please contact your System Administrator."
msgstr "Nie można zapisać preferencji użytkownika. Proszę skontaktować się z administratorem systemu."

#. ERR_VDA_TEAM_REL_NOT_ENABLED
msgid "DAL relationship: {0} cannot be used in team management because it is not enabled for team management."
msgstr "Relacja DAL: obiekt {0} nie może być używany w zarządzaniu zespołem, ponieważ nie jest włączony dla zarządzania zespołem."

#. ERR_VDA_TEAM_REL_KEY_NOTSAME
msgid "DAL relationship: {0} cannot be used in team management because source and target entity keys are different."
msgstr "Relacja DAL: obiekt {0} nie może być używany w zarządzaniu zespołem, ponieważ klucze encji źródłowej i docelowej są różne."

#. ERR_VDA_TEAM_REL_SOURCE_ATTRKEY
msgid "DAL relationship: {0} cannot be used in team management because source attribute key is not @key"
msgstr "Relacja DAL: obiekt {0} nie może być używany w zarządzaniu zespołem, ponieważ klucz atrybutu źródła nie ma wartości @key"

#. ERR_VDA_TEAM_REL_INVALID_ENTDEF
msgid "DAL relationship: {0} cannot be used in team management because target and source entity definition does not refer to: {1}."
msgstr "Relacja DAL: obiekt {0} nie może być używany w zarządzaniu zespołem, ponieważ definicje encji źródłowej i docelowej nie odnoszą się do: {1}."

#. INFO_VDD_CHOICE_DEF_LOADED
msgid "Directory Abstraction Choice List Definitions have been loaded into server lifetime memory"
msgstr "Definicje list wyboru abstrakcji katalogu zostały wczytane do pamięci ulotnej serwera"

#. INFO_VDD_CONFIG_DEF_LOADED
msgid "Directory Abstraction Configuration Definition have been loaded into server lifetime memory"
msgstr "Definicja konfiguracji abstrakcji katalogu została wczytana do pamięci ulotnej serwera"

#. INFO_VDD_ENTITY_DEF_LOADED
msgid "Directory Abstraction Entity Definitions have been loaded into server lifetime memory"
msgstr "Definicje encji abstrakcji katalogu zostały wczytane do pamięci ulotnej serwera"

#. INFO_VDD_QUERY_DEF_LOADED
msgid "Directory Abstraction Query Definitions have been loaded into server lifetime memory"
msgstr "Definicje zapytań abstrakcji katalogu zostały wczytane do pamięci ulotnej serwera"

#. INFO_VDD_RELATION_DEF_LOADED
msgid "Directory Abstraction Relationship List Definitions have been loaded into server lifetime memory"
msgstr "Definicje list relacji abstrakcji katalogu zostały wczytane do pamięci ulotnej serwera"

#. ERR_CACHE_STATE
msgid "Directory abstraction definition object: {0} is null, yet cacheholder reports that it is in the cache"
msgstr "Obiekt definicji abstrakcji katalogu: {0} ma wartość null, a pojemnik bufora zawiera informację, że jest on w pamięci podręcznej"

#. ERR_VDA_ENTITY_NOTFOUND
msgid "Entity/object not found: {0}."
msgstr "Nie znaleziono encji / obiektu: {0}."

#. ERR_ENTITY_NULL
msgid "Entity: [{0}] not found."
msgstr "Nie znaleziono encji: [{0}]."

#. ERR_VDA_BUILD_CONT_FILTER
msgid "Error building container search filter."
msgstr "Błąd podczas tworzenia filtra wyszukiwania kontenerów."

#. ERR_VDA_LOCKSMITH_CHECK
msgid "Error checking user for ldap administrator. Error: {0}"
msgstr "Błąd podczas sprawdzania użytkownika pod względem administratora ldap. Błąd: {0}"

#. ERR_LDAP_TIME
msgid "Error converting LDAP time attribute: {0}"
msgstr "Błąd podczas konwertowania atrybutu czasu LDAP: {0}"

#. ERR_VDA_CREATE_HIDDEN_ATTRIBUTE
msgid "Error creating object: {0}. Attempting to hide an attribute when not permitted by the definition: {1}."
msgstr "Błąd podczas tworzenia obiektu: {0}. Następuje próba ukrycia atrybutu, jeśli nie jest to dozwolone przez definicję: {1}."

#. ERR_VDA_CREATE_DEFINITION
msgid "Error creating object: {0}. Object definition does not allow create."
msgstr "Błąd podczas tworzenia obiektu: {0}. Definicja obiektu nie zezwala na tworzenie."

#. ERR_VDA_CREATE_MULTI_VALUED
msgid "Error creating object: {0}. The definition for attribute key {1} is single-valued, and multiple values exist."
msgstr "Błąd podczas tworzenia obiektu: {0}. Definicja dla klucza atrybutu {1} ma wartość pojedynczą, a istnieje wiele wartości."

#. ERR_VDA_CREATE_NO_OBJECT_CLASS_DEFINITION
msgid "Error creating object: {0}. The object definition {1}, does not specify any object classes."
msgstr "Błąd podczas tworzenia obiektu: {0}. Definicja obiektu {1} nie określa żadnych klas obiektów."

#. ERR_VDA_LDAP_CONNECTION
msgid "Error getting Ldap connection. Error: {0}"
msgstr "Błąd podczas pobierania połączenia LDAP. Błąd: {0}"

#. ERR_VDA_GET_TEAM_MANAGERS
msgid "Error getting a list of team managers for: {0}."
msgstr "Wystąpił błąd podczas pobierania listy menedżerów zespołu dla: {0}."

#. ERR_VDA_GET_TEAM_MEMBERS
msgid "Error getting a list of team members for: {0}."
msgstr "Błąd podczas pobierania listy członków zespołu dla: {0}."

#. ERR_VDM_DEFAULT_NAMING_ATTRIBUTE
msgid "Error getting the default LDAP naming attribute. Please contact your System Administrator."
msgstr "Błąd podczas pobierania domyślnego atrybutu nazywania LDAP. Proszę skontaktować się z administratorem systemu."

#. ERR_VDM_GUID_CAST
msgid "Error occured casting GUID for object Dn: {0} to binary. String (hex) value is: {1}."
msgstr "Wystąpił błąd rzutowania GUID dla obiektu o DN: {0} na wartość binarną. Wartość szesnastkowa ciągu to: {1}."

#. ERR_VDM_PARSING_PATH_VAL
msgid "Error occured parsing the Path Syntax value {0}"
msgstr "Wystąpił błąd podczas przetwarzania wartości składni ścieżki {0}"

#. ERR_VDM_PARSING_TYPED_NAME
msgid "Error occured parsing the Typed Name Syntax value {0}"
msgstr "Wystąpił błąd podczas przetwarzania wartości składni nazwy o typie {0}"

#. ERR_VDA_TEAM_MANAGER_SCOPE_AUTH
msgid "Error occurred checking manager: {0} authorization scope to team: {1}."
msgstr "Wystąpił błąd podczas sprawdzania zakresu uwierzytelniania menedżera: {0} do zespołu {1}."

#. ERR_VDA_OBJECT_TYPE_CHECK
msgid "Error occurred checking the object type for: {0}"
msgstr "Wystąpił błąd podczas sprawdzania typu obiektu dla: {0}"

#. ERR_CONVERTING_DATE
msgid "Error occurred converting last modify date: {0} on object DN: {1} from a UTC date format"
msgstr "Wystąpił błąd podczas konwertowania daty ostatniej modyfikacji: {0} obiektu o DN: {1}, z formatu daty UTC"

#. ERR_VDA_GET_TEAMS_MANAGED
msgid "Error occurred getting teams that the following user is a manager of: {0}."
msgstr "Wystąpił błąd podczas pobierania zespołów, których następujący użytkownik jest menedżerem: {0}."

#. ERR_VDD_XML_TO_OBJECT
msgid "Error occurred loading the abstract layer definition from the User Application driver (UAD). There are 2 things that can cause this: 1) The XML document stored in the XmlData attribute of the object instance: {0} is malformed or not in the expected format. This XML is packaged in the driver preconfig file and initialized when the UAD is created in iManager. The XML is maintained on the client side by the DAL eclipse plugin included with IDM Designer. 2) The file that maps the XML from the UAD abstract layer definition into the target application object is malformed. The map file is located in the User Application WAR under: {1}. This file is packaged with the product and does not change, unless inadvertently tampered with."
msgstr "Wystąpił błąd podczas wczytywania definicji warstwy abstrakcji ze sterownika aplikacji użytkownika (SAU). Możliwe są 2 przyczyny: 1) Dokument XML przechowywany w atrybucie XmlData instancji obiektu: {0} jest uszkodzony lub posiada nieoczekiwany format. Ten XML jest spakowany w pliku prekonfiguracji sterownika i inicjalizowany, jeśli SAU jest tworzony w programie iManager. Ten XML jest obsługiwany po stronie klienta przez wtyczkę DAL środowiska Eclipse dołączoną do programu IDM Designer. 2) Plik mapujący XML z definicji warstwy abstrakcyjnej SAU na docelowy obiekt aplikacji jest uszkodzony. Plik mapujący znajduje się w pliku WAR aplikacji użytkownika w: {1}. Ten plik jest pakowany z produktem i nie jest zmieniany, poza przypadkiem ręcznej ingerencji."

#. ERR_VDD_PARSING_DRIVER_EXPDATE
msgid "Error occurred parsing the expiration date value: {0} from the attribute: {1} in the User Application Driver: {2}. It is not returned in the expected format (i.e. expval#blah#blah, where expval is a signed integer number of seconds since Midnight, January 1, 1970)"
msgstr "Wystąpił błąd podczas przetwarzania wartości daty wygaśnięcia: {0} z atrybutu: {1} w sterowniku aplikacji użytkownika: {2}. Nie została zwrócona wartość w oczekiwanym formacie (tzn: expval#bla#bla, gdzie expval to całkowita liczba sekund od północy 1 stycznia 1970)"

#. ERR_VDD_MACRO_EXTENDER
msgid "Error occurred processing entity definition container macros using the class: {0} defined in the VirtualDataService-config/config.xml"
msgstr "Wystąpił błąd podczas przetwarzania makr kontenera definicji encji za pomocą klasy: {0}, zdefiniowanej w VirtualDataService-config/config.xml"

#. ERR_VDD_READING_DRIVER_EXPDATE
msgid "Error occurred reading the expiration date from the attribute: {0} in the User Application Driver: {1}"
msgstr "Wystąpił błąd podczas odczytywania daty wygaśnięcia z atrybutu: {0} w sterowniku aplikacji użytkownika: {1}"

#. ERR_VDM_GET_CACHE
msgid "Error occurred reading virtual definition object from server cache: {0}"
msgstr "Wystąpił błąd podczas odczytywania obiektu definicji wirtualnej z pamięci podręcznej serwera: {0}"

#. ERR_REGISTER_SCRIPT_ENGINE
msgid "Error occurred registering (or unregistering) object: {0} with script engine."
msgstr "Wystąpił błąd podczas rejestrowania (lub wyrejestrowania) obiektu: {0} z pomocą silnika skryptów."

#. ERR_VDM_DEFAULT_ATTRIBUTE_EXPR
msgid "Error occurred resolving the default value expression for attribute: {0}. ECMA Script expression [{1}]."
msgstr "Wystąpił błąd podczas określania wyrażenia wartości domyślnej dla atrybutu: {0}. Wyrażenie ECMA Script [{1}]."

#. ERR_VDA_BINARY_ENV_SETTING
msgid "Error occurred setting JNDI LDAP context environment variable java.naming.ldap.attributes.binary for entity definition: {0}."
msgstr "Wystąpił błąd podczas ustawiania zmiennej środowiskowej java.naming.ldap.attributes.binary kontekstu JNDI LDAP dla definicji encji: {0}."

#. ERR_VDA_IS_TEAM_MANAGER_OF
msgid "Error occurred trying to resolve if the following user was a team manager: {0} of team: {1}."
msgstr "Wystąpił błąd podczas określania, czy poniższy użytkownik był menedżerem: {0} zespołu: {1}."

#. ERR_VDA_IS_TEAM_MANAGER
msgid "Error occurred trying to resolve if the following user was a team manager: {0}."
msgstr "Wystąpił błąd podczas próby określenia, czy poniższy użytkownik był menedżerem zespołu: {0}."

#. ERR_VDM_REL_TYPE_NULL
msgid "Error occurred, null relationship type specified"
msgstr "Wystąpił błąd. Określono typ relacji o wartości null."

#. ERR_VDM_DEFINITION_ATTRIBUTE_NULL
msgid "Error occurred, the Virtual Entity Definition Attribute key (object attribute name) was null"
msgstr "Wystąpił błąd. Klucz atrybutu definicji wirtualnej encji (nazwa atrybutu obiektu) ma wartość null."

#. ERR_VDM_DEFINITION_NOTFOUND
msgid "Error occurred, the Virtual Entity Definition key (object name) \\'\\'{0}\\'\\' was not found"
msgstr "Wystąpił błąd. Nie znaleziono klucza definicji wirtualnej relacji (nazwy obiektu) \\'\\'{0}\\'\\'."

#. ERR_VDM_DEFINITION_NULL
msgid "Error occurred, the Virtual Entity Definition key (object name) was null"
msgstr "Wystąpił błąd. Klucz definicji wirtualnej relacji (nazwa obiektu) ma wartość null."

#. ERR_VDM_REL_DEFINITION_NULL
msgid "Error occurred, the Virtual Relationship Definition was null for key {0}"
msgstr "Wystąpił błąd. Definicja wirtualnej relacji miała wartość null dla klucza {0}."

#. ERR_VDA_QUERY_DEFINITION_MISSING
msgid "Error performing query. Could not find object definition attribute key: {0} in definition: {1}."
msgstr "Wystąpił błąd podczas wykonywania zapytania. Nie można było znaleźć klucza atrybutu definicji obiektu: {0} w definicji: {1}."

#. ERR_VDA_QUERY_DEFINITION_ENABLED
msgid "Error performing query. The object definition attribute key: {0} in definition: {1}, is not enabled or is not readable."
msgstr "Wystąpił błąd podczas wykonywania zapytania. Klucz atrybutu definicji obiektu: {0} w definicji: {1} nie jest włączony lub nie można go odczytać."

#. ERR_VDA_QUERY_DEFINITION_SEARCHABLE
msgid "Error performing query. The object definition attribute key: {0} in definition: {1}, is not searchable."
msgstr "Wystąpił podczas wykonywania zapytania. Nie można przeszukać klucza atrybutu definicji obiektu: {0} w definicji: {1}."

#. ERR_VDA_QUERY_SIZE_LIMIT
msgid "Error performing query: number of results found exceeds the search size limit. Narrow the scope of the search or increase the search size limit."
msgstr "Wystąpił błąd podczas wykonywania zapytania: liczba znalezionych wyników przekracza limit rozmiaru wyszukiwania. Proszę zawęzić zakres wyszukiwania lub zwiększyć limit rozmiaru wyszukiwania."

#. ERR_VDA_QUERY_TIME_LIMIT
msgid "Error performing query: search exceeds specified time limit. Narrow the scope of the search or increase the search time limit."
msgstr "Wystąpił błąd podczas wykonywania zapytania: czas wyszukiwania przekroczył określony limit. Proszę zawęzić zakres wyszukiwania lub zwiększyć limit czasu."

#. ERR_VDD_PUT_CACHE
msgid "Error putting the following object key into the custom VDATA cacheholder in server lifetime cache: {0}"
msgstr "Wystąpił błąd podczas umieszczania poniższego klucza obiektu w pojemniku bufora VDATA w podręcznej pamięci operacyjnej serwera: {0}."

#. ERR_VDA_SS_READ
msgid "Error reading SecretStore {0}, on Object: {1}."
msgstr "Wystąpił błąd podczas odczytu SecretStore {0} w obiekcie: {1}."

#. ERR_VDD_LDAP_SEARCH
msgid "Error reading/searching for abstract layer definition(s) from identity vault, check the following query parameters and verify that they match your identity vault schema and user application driver configuration - Search filter: {0}, Search root: {1}"
msgstr "Wystąpił błąd podczas odczytu / wyszukiwania definicji warstwy abstrakcji z repozytorium ról. Proszę sprawdzić poniższe parametry zapytania i zweryfikować, czy pasują do schematu repozytorium tożsamości i konfiguracji sterownika aplikacji użytkownika - filtr wyszukiwania: {0}, katalog główny wyszukiwania: {1}."

#. ERR_VDD_REFRESH_CACHE
msgid "Error refreshing abstract layer data definition cache"
msgstr "Wystąpił błąd podczas odświeżania pamięci podręcznej danych warstwy abstrakcji."

#. ERR_VDA_SS_REMOVE
msgid "Error removing SecretStore {0}, on Object: {1}."
msgstr "Wystąpił błąd podczas usuwania SecretStore {0} w obiekcie: {1}."

#. ERR_VDA_REMOVE_DEFINITION
msgid "Error removing object: {0}. object definition does not allow remove."
msgstr "Wystąpił błąd podczas usuwania obiektu: {0}. Definicja obiektu nie zezwala na usuwanie."

#. ERR_VDA_SS_UPDATE
msgid "Error updating SecretStore {0}, on Object: {1}."
msgstr "Wystąpił błąd podczas aktualizacji SecretStore {0} w obiekcie: {1}."

#. ERR_VDA_UPDATE_HIDDEN_ATTRIBUTE
msgid "Error updating object: {0}. Attempting to hide an attribute({1}) when not permitted by the definition."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Próba ukrycia atrybutu ({1}), gdy jest to niedozwolone przez definicję."

#. ERR_VDA_UPDATE_DEFINITION
msgid "Error updating object: {0}. Object definition does not allow update."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Definicja obiektu nie zezwala na aktualizację."

#. ERR_VDA_UPDATE_EDITABLE_ATTRIBUTE
msgid "Error updating object: {0}. The definition for attribute key {1} does not permit updates."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Definicja klucza atrybutu {1} nie zezwala na aktualizacje."

#. ERR_VDA_UPDATE_ENABLED_ATTRIBUTE
msgid "Error updating object: {0}. The definition for attribute key {1} is not enabled."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Definicja klucza atrybutu {1} nie jest włączona."

#. ERR_VDA_UPDATE_MULTI_VALUED
msgid "Error updating object: {0}. The definition for attribute key {1} is single-valued, and multiple values exist."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Definicja klucza atrybutu {1} jest jednowartościowa, a istnieje wiele wartości."

#. ERR_VDA_UPDATE_HIDDEN_ATTRIBUTE_OWNER
msgid "Error updating object: {0}. The hidden status of an attribute({1}) can only be changed by an administrator or the owner of the object."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Ukryty stan atrybutu ({1}) może być zmieniany tylko przez administratora lub właściciela obiektu."

#. ERR_VDA_UPDATE_HIDDEN_OWNER
msgid "Error updating object: {0}. The hidden status of an object can only be changed by an administrator or the owner of the object."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Ukryty stan obiektu może być zmieniany tylko przez administratora lub właściciela obiektu."

#. ERR_VDA_UPDATE_NON_AUX_CLASS
msgid "Error updating object: {0}. The object class: {1}, is not an auxiliary class. This error can also occur if you issue an updateEntity(...) without first doing a getEntity(...)."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Klasa obiektu: {1} nie jest klasą dodatkową. Błąd ten może zdarzyć się także po wywołaniu funkcji updateEntity(...) bez uprzedniego wywołania getEntity(...)."

#. ERR_VDA_UPDATE_NO_OBJECT_CLASS_DEFINITION
msgid "Error updating object: {0}. The object definition {1}, does not specify any object classes."
msgstr "Wystąpił błąd podczas aktualizacji obiektu: {0}. Definicja obiektu {1} nie określa żadnych klas obiektu."

#. ERR_VDA_UPDATE_CREDENTIALS
msgid "Error updating user password: {0}. Error: {1}"
msgstr "Wystąpił błąd podczas aktualizacji hasła użytkownika: {0}. Błąd: {1}."

#. ERR_DOM_CREATION
msgid "Error when creating now XML DOM. Can\\'t complete request."
msgstr "Wystąpił błąd podczas tworzenia modelu XML DOM. Nie można zakończyć wniosku."

#. ERR_XPATH_INVALID_EXPR
msgid "Invalid XPath Expression: {0}"
msgstr "Nieprawidłowe wyrażenie XPath: {0}"

#. ERR_VDM_INVALID_BINARY_VAL
msgid "Invalid object type for DAL Binary attribute: \\'\\'{0}\\'\\' . Change the data type to a String in the IDM Designer. Or, if you are using this attribute to persist image(s) change the UI control format type to an Image."
msgstr "Nieprawidłowy typ obiektu dla atrybutu binarnego DAL: \\'\\'{0}\\'\\'. Proszę zmienić w programie IDM Designer typ danych na String lub zmienić typ formaty kontrolki interfejsu użytkownika na Image, jeśli atrybut ten używany jest do zachowywania obrazów."

#. ERR_VDM_INVALID_PATH_VAL
msgid "Invalid object type for DAL Path attribute: \\'\\'{0}\\'\\' . Must be Java type: com.novell.srvprv.spi.vdata.util.PathSyntax"
msgstr "Nieprawidłowy typ obiektu dla atrybutu DAL Path: \\'\\'{0}\\'\\'. Wymagany typ Java: com.novell.srvprv.spi.vdata.util.PathSyntax."

#. ERR_VDM_INVALID_TIME_VAL
msgid "Invalid object type for DAL Time attribute: \\'\\'{0}\\'\\' . Must be Java type: java.util.Date"
msgstr "Nieprawidłowy typ obiektu dla atrybutu DAL Time: \\'\\'{0}\\'\\'. Wymagany typ Java: java.util.Date."

#. ERR_VDM_INVALID_TYPEDNAME_VAL
msgid "Invalid object type for DAL Typed Name attribute: \\'\\'{0}\\'\\' . Must be Java type: com.novell.srvprv.spi.vdata.util.TypedNameSyntax"
msgstr "Nieprawidłowy typ obiektu dla atrybutu DAL Typed Name: \\'\\'{0}\\'\\'. Wymagany typ Java: com.novell.srvprv.spi.vdata.util.TypedNameSyntax."

#. ERR_VDA_INVALID_TYPE
msgid "Invalid value for attribute[type]: \\'\\'{0}\\'\\' on object \\'\\'{1}\\'\\'"
msgstr "Nieprawidłowa wartość dla atrybutu [typ]: \\'\\'{0}\\'\\' dla obiektu \\'\\'{1}\\'\\'."

#. ERR_VDA_CREATE_LDAP
msgid "Ldap error creating object: {0}. Error: {1}"
msgstr "Błąd LDAP podczas tworzenia obiektu: {0}. Błąd: {1}."

#. ERR_VDA_CREATE_LDAP_ALREADY_BOUND
msgid "Ldap error creating object: {0}. Object already exists."
msgstr "Błąd LDAP podczas tworzenia obiektu: {0}. Obiekt już istnieje."

#. ERR_VDA_QUERY_NO_OBJECT_CLASS_DEFINITION
msgid "Ldap error creating query. The object definition {0}, does not specify any object classes."
msgstr "Błąd LDAP podczas tworzenia zapytania. Definicja obiektu {0} nie określa żadnej klasy obiektów."

#. ERR_VDA_GET_LDAP_ATTRIBUTES
msgid "Ldap error getting attributes for object: {0}. Error: {1}"
