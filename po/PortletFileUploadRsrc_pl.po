#. PROCESSING_FAILED
msgid "Processing of {0} request failed: {1}"
msgstr "Przetwarzanie wniosku {0} nie powiodło się: {1}"

#. REQ_PARAMETER
msgid "Require parameter"
msgstr "Wymagany parametr"

#. INVALID_CONTENT_TYPE
msgid "The request doesn\\'\\'t contain a {0} or {1} stream, content type header is {2}"
msgstr "Wniosek nie zawiera strumienia {0} lub {1}, typem zawartości nagłówka jest {2}"

#. SIZE_EXCEEDS_RANGE
msgid "The request was rejected because it\\'s size exceeds allowed range"
msgstr "Wniosek został odrzucony, ponieważ przekracza dopuszczalny rozmiar"

#. SIZE_IS_UNKNOWN
msgid "The request was rejected because it\\'s size is unknown"
msgstr "Wniosek został odrzucony, ponieważ nie jest znany jego rozmiar"

#. NO_MULTIPART_BOUNDARY
msgid "The request was rejected because no multipart boundary was found"
msgstr "Wniosek został odrzucony, ponieważ nie znaleziono granicy multipart"
