#. JSP_HELPPAGE_DEFAULT_PAGETITLE
msgid "Help: Challenge Response"
msgstr "Pomoc: Odpowiedź na wyzwanie"

#. JSP_HELPPAGE_DEFAULT_CONTENT
msgid "\\u003ch1\\u003eChallenge Response\\u003c/h1\\u003e \\u003cbr\\u003e \\u003cp\\u003eThis portlet prompts you to maintain your challenge questions and responses, which are used to verify your identity during login when you have forgotten your password.\\u003c/p\\u003e \\u003cp\\u003e&nbsp;\\u003c/p\\u003e \\u003cp\\u003e\\u003cb\\u003eFor more information\\u003c/b\\u003e, see the \\u003ca href=\\'http://www.novell.com/documentation/idm35/ugpro/data/ugproidentitypassword.html\\' target=\\'_blank\\'\\u003e documentation on performing password management\\u003c/a\\u003e in the \\u003ci\\u003eIdentity Manager User Application: User Guide\\u003c/i\\u003e on the Novell Web site.\\u003c/p\\u003e"
msgstr "\\u003ch1\\u003eOdpowiedź na wyzwanie\\u003c/h1\\u003e \\u003cbr\\u003e \\u003cp\\u003ePortlet ten wzywa do utworzenia pytań o wyzwania i odpowiedzi, które są używane do weryfikacji tożsamości podczas logowania, w przypadku zapomnienia hasła.\\u003c/p\\u003e \\u003cp\\u003e&nbsp;\\u003c/p\\u003e \\u003cp\\u003e\\u003cb\\u003eWięcej informacji\\u003c/b\\u003e - patrz \\u003ca href=\\'http://www.novell.com/documentation/idm35/ugpro/data/ugproidentitypassword.html\\' target=\\'_blank\\'\\u003e dokumentacja dotycząca zarządzania hasłami\\u003c/a\\u003e w \\u003ci\\u003ePodręczniku użytkownika aplikacji Identity Manager \\u003c/i\\u003e na stronie internetowej firmy Novell.\\u003c/p\\u003e"
