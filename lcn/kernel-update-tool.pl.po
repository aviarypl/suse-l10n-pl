#: kernel-update-tool.in:78
#, perl-format
msgid "Usage: %s [options] --status"
msgstr "Użycie: %s [opcje] --status"

#: kernel-update-tool.in:79
#, perl-format
msgid "       %s [options] {--install|--upgrade} {kernel} [driver] ..."
msgstr "       %s [opcje] {--install|--upgrade} {kernel} [sterownik] ..."

#: kernel-update-tool.in:80
msgid "Options are:"
msgstr "Opcje:"

#: kernel-update-tool.in:81
msgid ""
"--version kernelrelease\n"
"\tThe kernel release (uname -r) that the status command applies to.\n"
"\tThe kernel release that the install and upgrade actions install\n"
"\tand upgrade from.\n"
msgstr ""
"--version wydanie_jądra\n"
"\tWydanie jądra (uname -r), którego dotyczy polecenie stanu.\n"
"\tWydanie jądra, którego dotyczą akcje install, upgrade oraz install\n"
"\ti upgrade from.\n"

#: kernel-update-tool.in:86
msgid ""
"--non-interactive\n"
"\tDo not ask any questions.\n"
msgstr ""
"--non-interactive\n"
"\tNie zadawaj żadnych pytań.\n"

#: kernel-update-tool.in:89
msgid ""
"--dry-run\n"
"\tPerform everything that would be done during install or upgrade\n"
"\twithout actually installing or upgrading any packages.\n"
msgstr ""
"--dry-run\n"
"\tWykonaj wszystko co byłoby zrobione w czasie instalacji albo aktualizacji\n"
"\tbez instalowania lub aktualizowania naprawdę jakichkolwiek pakietów.\n"

#: kernel-update-tool.in:93
msgid ""
"--verbose\n"
"\tBe more verbose and send some some debug output to standard error.\n"
msgstr ""
"--verbose\n"
"\tBądź bardziej gadatliwy i wyślij nieco informacji diagnostycznych na stderr.\n"

#: kernel-update-tool.in:96
msgid ""
"--download-driver-packages={yes,no,ask}\n"
"\tSets whether to attempt to download kernel driver packages\n"
"\tfrom the locations defined in /var/lib/YaST2/download/*\n"
"\tand /var/lib/YaST2/download.conf.\n"
msgstr ""
"--download-driver-packages={yes,no,ask}\n"
"\tOkreśl czy chcesz próbować pobierać pakiety sterowników jądra\n"
"\tz lokalizacji zdefiniowanych w /var/lib/YaST2/download/*\n"
"\ti /var/lib/YaST2/download.conf.\n"

#: kernel-update-tool.in:101
msgid ""
"--reuse-driver-packages={yes,no,ask}\n"
"\tSets whether to attempt to reuse kernel driver packages from the\n"
"\tprevious kernel.\n"
msgstr ""
"--reuse-driver-packages={yes,no,ask}\n"
"\tOkreśl czy próbować używać pakiety ze sterownikami jądra\n"
"\tz poprzedniego jądra.\n"

#: kernel-update-tool.in:105
msgid ""
"--require-signed-packages={yes,no,ask}\n"
"\tWhether to require packages to be signed.\n"
msgstr ""
"--require-signed-packages={yes,no,ask}\n"
"\tCzy wymagać podpisanych pakietów.\n"

#: kernel-update-tool.in:108
msgid ""
"--parse-markers\n"
"\tMake the script output more easily parseable (for scripting).\n"
msgstr ""
"--parse-markers\n"
"\tSpraw aby wyjście skryptu było łatwiej parsowalne (do przetwarzania skryptowego).\n"

