#: ../src/logout-dialog.c:138
#: ../src/logout-dialog.c:156
msgid "Log Out"
msgstr "Wylogowanie"

#: ../src/logout-dialog.c:147
msgid "Shutdown"
msgstr "Wyłączenie komputera"

#: ../src/logout-dialog.c:286
msgid "_Log Out"
msgstr "_Wyloguj"

#: ../src/logout-dialog.c:287
msgid "Ends your session and logs you out."
msgstr "Kończy sesję użytkownika i wylogowuje z systemu."

#: ../src/logout-dialog.c:324
msgid "Loc_k Screen"
msgstr "Zablo_kuj ekran"

#: ../src/logout-dialog.c:325
msgid "Locks this computer's screen."
msgstr "Blokuje ekran komputera."

#: ../src/logout-dialog.c:363
msgid "_Shutdown"
msgstr "W_yłącz komputer"

#: ../src/logout-dialog.c:364
msgid "Ends your session and turns off the computer."
msgstr "Kończy sesji użytkownika i wyłącza komputer."

#: ../src/logout-dialog.c:404
msgid "_Restart"
msgstr "_Uruchom komputer ponownie"

#: ../src/logout-dialog.c:405
msgid "Ends your session and restarts the computer."
msgstr "Kończy sesję użytkownika i ponowne uruchomia komputer."

#: ../src/logout-dialog.c:436
msgid "Slee_p"
msgstr "_Przejdź w stan gotowości"

#: ../src/logout-dialog.c:437
msgid "Suspends your session quickly, using minimal power while the computer stands by."
msgstr "Wstrzymuje sesję użytkownika i przechodzi w stan gotowości, aby zminimalizować pobór energii."

#: ../src/logout-dialog.c:467
msgid "Hi_bernate"
msgstr "Zahi_bernuj komputer"

#: ../src/logout-dialog.c:468
msgid "Suspends your session, using no power until the computer is restarted."
msgstr "Wstrzymuje sesję użytkownika i przechodzi w stan uśpienia, w którym energia nie jest w ogóle pobierana aż do następnego uruchomienia komputera."

#: ../src/logout-dialog.c:496
msgid "S_witch User"
msgstr "Przełącz _użytkownika"

#: ../src/logout-dialog.c:497
msgid "Suspends your session, allowing another user to log in and use the computer."
msgstr "Wstrzymuje sesję bieżącego użytkownika umożliwiając zalogowanie się innemu użytkownikowi."

#: ../src/switch-user-dialog.c:290
#: ../src/switch-user-dialog.c:842
msgid "Select a User"
msgstr "Wybierz użytkownika"

#: ../src/switch-user-dialog.c:307
#: ../src/switch-user-dialog.c:859
msgid "_Switch User"
msgstr "P_rzełącz użytkownika"

#: ../src/switch-user-dialog.c:743
msgid "Logged in"
msgstr "Zalogowany"

