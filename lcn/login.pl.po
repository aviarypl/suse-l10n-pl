#: src/lastlog.c:82
#, c-format
msgid ""
"Usage: lastlog [options]\n"
"\n"
"Options:\n"
"  -b, --before DAYS\tprint only lastlog records older than DAYS\n"
"  -h, --help\t\tdisplay this help message and exit\n"
"  -t, --time DAYS\tprint only lastlog records more recent than DAYS\n"
"  -u, --user LOGIN\tprint lastlog record for user with specified LOGIN\n"
"  -v, --version\tprint version number and exit\n"
"\n"
msgstr ""
"Użycie: lastlog [opcje]\n"
"\n"
"Opcje:\n"
"  -b, --before DNI\twyświetla tylko wpisy lastlog starsze niż DNI\n"
"  -h, --help\t\twyświetla ten opis i kończy działanie\n"
"  -t, --time DNI\twyświetla tylko wpisy lastlog nie starsze niż DNI\n"
"  -u, --user LOGIN\twyświetla wpis lastlog tylko dla użytkownika o nazwie LOGIN\n"
"  -v, --version\twyświetla numer wersji i kończy działanie\n"
"\n"

