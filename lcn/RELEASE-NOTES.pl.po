#: RELEASE-NOTES.xml:482(screen)
#, no-wrap
msgid "xrandr -o 0 &amp;&amp; xsetwacom set \"Mouse[7]\" RotateNONE"
msgstr ""

#: RELEASE-NOTES.xml:485(para)
msgid "For 90° rotation (clockwise, portrait):"
msgstr ""

#: RELEASE-NOTES.xml:486(screen)
#, no-wrap
msgid "xrandr -o 3 &amp;&amp; xsetwacom set \"Mouse[7]\" Rotate CW"
msgstr ""

#: RELEASE-NOTES.xml:489(para)
msgid "For 180° rotation (landscape):"
msgstr ""

#: RELEASE-NOTES.xml:490(screen)
#, no-wrap
msgid " xrandr -o 2 &amp;&amp; xsetwacom set \"Mouse[7]\" Rotate HALF"
msgstr ""

#: RELEASE-NOTES.xml:493(para)
msgid "For 270° rotation (counterclockwise, portrait):"
