#: src/pam-config.c:68
#, c-format
msgid "Try `%s --help' or `%s --usage' for more information.\n"
msgstr "Użyj `%s --help' lub `%s --usage', aby dowiedzieć się więcej.\n"

#: src/pam-config.c:76
#, c-format
msgid "%s - create PAM config files\n\n"
msgstr "%s - tworzy pliki konfiguracyjne PAM\n\n"

#: src/pam-config.c:78
msgid "  -a, --add         Add options/PAM modules\n"
msgstr "  -a, --add         Dodaje opcje/moduły PAM\n"

#: src/pam-config.c:79
msgid "  -c, --create      Create new configuration\n"
msgstr "  -c, --create      Tworzy nową konfigurację\n"

#: src/pam-config.c:80
msgid "  -d, --delete      Remove options/PAM modules\n"
msgstr "  -d, --delete      Usuwa opcje/moduły PAM\n"

#: src/pam-config.c:81
msgid "      --confdir     Use a custom configuration directory\n"
msgstr "      --confdir     Użyj własnego katalogu konfiguracji\n"

#: src/pam-config.c:83
msgid "      --initialize  Convert old config and create new one\n"
msgstr "      --initialize  Konweruje starą konfigurację na nową\n"

#: src/pam-config.c:85
msgid "      --service config  Service to modify config of\n"
msgstr "  --service config  Usługa modyfikująca konfiguracje\n"

#: src/pam-config.c:87
msgid "      --update      Read current config and write them new\n"
msgstr "      --update      Odczytuje bieżącą konfigurację i zapisuje ją na nowo\n"

#: src/pam-config.c:89
msgid "  -q, --query       Query for installed modules and options\n"
msgstr "  -q, --query       Wyszukuje zainstalowane moduły i opcje\n"

#: src/pam-config.c:91
msgid "      --help        Give this help list\n"
msgstr "  --help         Wyświetla tę pomoc\n"

#: src/pam-config.c:92
msgid "  -u, --usage       Give a short usage message\n"
msgstr "  --usage        Pokazuje krótką wiadomość dot. używania\n"

#: src/pam-config.c:93
msgid "  -v, --version     Print program version\n"
msgstr "  --version      Wyświetla wersję programu\n"

#: src/pam-config.c:96
msgid "Global Module Options:\n"
msgstr "Globalne opcje modułu:\n"

#: src/pam-config.c:97
msgid "      The global modules get inserted into the \n      common-{account,auth,password,session} files which are included by\n      the single service files.\n"
msgstr "      Globalne moduły są dołączane do \n      ogólnych plików-{konta,autoryzacji,hasła,sesji} które są\n      włączane do pojedynczych plików usług.\n"

#: src/pam-config.c:102
msgid "Single Service Module Options:\n"
msgstr "Opcje modułu dla pojedynczej usługi:\n"

#: src/pam-config.c:103
msgid "      These modules can only be added to single service files.\n"
msgstr "      Te moduły mogą by dodane tylko do pojedynczych plików usług.\n"

