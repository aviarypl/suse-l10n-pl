#: ../src/logout-dialog.c:147 ../src/logout-dialog.c:167
msgid "Log Out"
msgstr "Wylogowanie"

#: ../src/logout-dialog.c:157
msgid "Shutdown"
msgstr "Wyłączenie komputera"

#: ../src/logout-dialog.c:286
msgid "_Log Out"
msgstr "Wyl_oguj"

#: ../src/logout-dialog.c:287
msgid "Ends your session and logs you out."
msgstr "Kończy sesję i wylogowuje z systemu."

#: ../src/logout-dialog.c:324
msgid "Loc_k Screen"
msgstr "Za_blokuj ekran"

#: ../src/logout-dialog.c:325
msgid "Locks this computer's screen."
msgstr "Blokuje ekran komputera."

#: ../src/logout-dialog.c:363
msgid "_Shutdown"
msgstr "_Wyłącz komputer"

#: ../src/logout-dialog.c:364
msgid "Ends your session and turns off the computer."
msgstr "Kończy sesję i wyłącza komputer."

#: ../src/logout-dialog.c:404
msgid "_Restart"
msgstr "_Uruchom komputer ponownie"

#: ../src/logout-dialog.c:405
msgid "Ends your session and restarts the computer."
msgstr "Kończy sesję i uruchamia komputer ponownie."

#: ../src/logout-dialog.c:436
msgid "Slee_p"
msgstr "_Przejdź w stan wstrzymania"

#: ../src/logout-dialog.c:437
msgid "Suspends your session quickly, using minimal power while the computer stands by."
msgstr "Wstrzymuje sesję i przechodzi w stan wstrzymania z minimalnym poborem energii."

#: ../src/logout-dialog.c:467
msgid "Hi_bernate"
msgstr "Przejdź w stan _hibernacji"

#: ../src/logout-dialog.c:468
msgid "Suspends your session, using no power until the computer is restarted."
msgstr "Wstrzymuje sesję i przechodzi w stan uśpienia bez poboru energii."

#: ../src/logout-dialog.c:496
msgid "S_witch User"
msgstr "Przełącz _użytkownika"

#: ../src/logout-dialog.c:497
msgid "Suspends your session, allowing another user to log in and use the computer."
msgstr "Wstrzymuje sesję bieżącego użytkownika i pozwala zalogować się innemu użytkownikowi."

#: ../src/switch-user-dialog.c:290 ../src/switch-user-dialog.c:842
msgid "Select a User"
msgstr "Użytkownik"

#: ../src/switch-user-dialog.c:307 ../src/switch-user-dialog.c:859
msgid "_Switch User"
msgstr "_Przełącz"

#: ../src/switch-user-dialog.c:743
msgid "Logged in"
msgstr "Zalogowany"

