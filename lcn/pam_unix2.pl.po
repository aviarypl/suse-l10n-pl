#: src/support.c:157
msgid "login: "
msgstr "Login:"

#: src/support.c:171 src/support.c:218
msgid "Password: "
msgstr "Hasło: "

#: src/unix_acct.c:100 src/unix_acct.c:148
msgid "Your password has expired. Choose a new password."
msgstr "Hasło jest przeterminowane. Proszę podać nowe."

#: src/unix_acct.c:127
msgid "Password change requested. Choose a new password."
msgstr "Zażądano zmiany hasła. Proszę podać nowe."

#: src/unix_acct.c:311
#, c-format
msgid "Your password will expire in %ld days."
msgstr "Hasło wygaśnie za %ld dni."

#: src/unix_acct.c:315
msgid "Your password will expire tomorrow."
msgstr "Hasło wygaśnie jutro."

#: src/unix_acct.c:318
msgid "Your password will expire within 24 hours."
msgstr "Hasło wygaśnie w ciągu 24 godzin."

#: src/unix_auth.c:298
msgid "Permissions on the password database may be too restrictive."
msgstr "Uprawnienia dotyczące bazy haseł mogą być zbyt restrykcyjne."

#: src/unix_passwd.c:83
msgid "Old Password: "
msgstr "Stare hasło: "

#: src/unix_passwd.c:84
msgid "New Password: "
msgstr "Nowe hasło: "

#: src/unix_passwd.c:85
msgid "Reenter New Password: "
msgstr "Proszę podać nowe hasło ponownie: "

#: src/unix_passwd.c:216
msgid "You can only change local passwords."
msgstr "Można zmieniać tylko lokalne hasła."

#: src/unix_passwd.c:287
#, c-format
msgid "Less then %d weeks since the last change."
msgstr "Mniej niż %d tygodni od ostatniej zmiany."

#: src/unix_passwd.c:291
#, c-format
msgid "Less then %d days since the last change."
msgstr "Mniej niż %d dni od ostatniej zmiany."

#: src/unix_passwd.c:343 src/unix_passwd.c:358
msgid "Password change aborted."
msgstr "Zmiana hasła przerwana."

#: src/unix_passwd.c:372
msgid "Passwords do not match."
msgstr "Hasła nie zgadzają się."

#: src/unix_passwd.c:397
msgid "Error: Password NOT changed."
msgstr "Błąd: Hasło NIE zostało zmienione."

#: src/unix_passwd.c:406
msgid "Password changed."
msgstr "Hasło zmienione."

#: src/unix_passwd.c:423
#, c-format
msgid "Cannot find the master ypserver: %s"
msgstr "Nie można znaleźć głównego serwera ypserver: %s"

#: src/unix_passwd.c:431
#, c-format
msgid "yppasswdd not running on NIS master %s"
msgstr "yppasswdd nie działa na głównym serwerze NIS %s"

#: src/unix_passwd.c:437
msgid "yppasswd daemon running on illegal port."
msgstr "Demon yppasswd działa na nielegalnym porcie."

#: src/unix_passwd.c:488
#, c-format
msgid "Cannot open %s for reading: %s"
msgstr "Nie można otworzyć pliku %s do odczytu: %s"

#: src/unix_passwd.c:497
#, c-format
msgid "Unable to obtain entropy from %s"
msgstr "Nie można uzyskać entropii z %s"

#: src/unix_passwd.c:512
msgid "Unable to generate a salt. Check your crypt settings."
msgstr "Nie można wygenerować wartości salt. Proszę sprawdzić ustawienia szyfrowania."

#: src/unix_passwd.c:638
msgid "Cannot create salt for standard crypt"
msgstr "Nie można utworzyć wartości salt. Proszę sprawdzić ustawienia szyfrowania."

#: src/unix_passwd.c:650
msgid "Cannot create salt for MD5 crypt"
msgstr "Nie można utworzyć wartości salt dla szyfrowania MD5"

#: src/unix_passwd.c:663
msgid "Cannot create salt for bigcrypt"
msgstr "Nie można utworzyć wartości salt dla bigcrypt"

#: src/unix_passwd.c:669
msgid "No support for bigcrypt included"
msgstr "Brak obsługi bigcrypt"

#: src/unix_passwd.c:681
msgid "Cannot create salt for blowfish crypt"
msgstr "Nie można utworzyć wartości salt dla szyfrowania blowfish"

#: src/unix_passwd.c:687
msgid "No support for blowfish included"
msgstr "Brak obsługi blowfish"

#: src/unix_passwd.c:699
msgid "crypt_r() returns NULL pointer"
msgstr "crypt_r() zwraca wskaźnik NULL"

#: src/unix_passwd.c:722
msgid "Cannot lock password file: already locked."
msgstr "Nie można zablokować pliku passwd: już jest zablokowany."

#: src/unix_passwd.c:749 src/unix_passwd.c:810 src/unix_passwd.c:893
#: src/unix_passwd.c:954
#, c-format
msgid "Cannot open %s: %m"
msgstr "Nie można otworzyć %s: %m"

#: src/unix_passwd.c:756 src/unix_passwd.c:900
#, c-format
msgid "Cannot stat %s: %m"
msgstr "Brak dostępu do %s: %m"

#: src/unix_passwd.c:784 src/unix_passwd.c:928
#, c-format
msgid "Cannot create temp file (%s): %m"
msgstr "Nie można utworzyć pliku tymczasowego (%s): %m"

#: src/unix_passwd.c:792 src/unix_passwd.c:936
#, c-format
msgid "Cannot change permssions of %s: %m"
msgstr "Nie można zmienić uprawnień dla %s: %m"

#: src/unix_passwd.c:797 src/unix_passwd.c:940
#, c-format
msgid "Cannot change owner/group of %s: %m"
msgstr "Nie można zmienić właściciela/grupy dla %s: %m"

#: src/unix_passwd.c:840
#, c-format
msgid "Error while writing new shadow file: %m"
msgstr "Błąd podczas zapisu nowego pliku shadow: %m"

#: src/unix_passwd.c:850
#, c-format
msgid "Error while closing old shadow file: %m"
msgstr "Błąd podczas zamykania starego pliku shadow: %m"

#: src/unix_passwd.c:858
#, c-format
msgid "Error while closing temporary shadow file: %m"
msgstr "Błąd podczas zamykania tymczasowego pliku shadow: %m"

#: src/unix_passwd.c:865 src/unix_passwd.c:1005
#, c-format
msgid "Cannot create backup file of %s: %m"
msgstr "Nie można utworzyć pliku zapasowego %s: %m"

#: src/unix_passwd.c:980
#, c-format
msgid "Error while writing new password file: %m"
msgstr "Błąd podczas zapisu nowego pliku passwd: %m"

#: src/unix_passwd.c:990
#, c-format
msgid "Error while closing old password file: %m"
msgstr "Błąd podczas zamykania starego pliku passwd: %m"

#: src/unix_passwd.c:998
#, c-format
msgid "Error while closing temporary password file: %m"
msgstr "Błąd podczas zamykania tymczasowego pliku passwd: %m"

#: src/unix_passwd.c:1037
#, c-format
msgid "Changing NIS password for %s on %s."
msgstr "Zmiana hasła NIS dla %s na %s."

#: src/unix_passwd.c:1052
msgid "Error while changing the NIS password."
msgstr "Błąd podczas zmiany hasła NIS."

