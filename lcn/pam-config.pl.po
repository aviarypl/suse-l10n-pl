msgid "      --initialize  Convert old config and create new one\n"
msgstr "      --initialize  Konweruje starą konfigurację na nową\n"

#: src/pam-config.c:84
msgid "      --service config  Service to modify config of\n"
msgstr "  --service config  Usługa modyfikująca konfiguracje\n"

#: src/pam-config.c:86
msgid "      --update      Read current config and write them new\n"
msgstr "      --update      Odczytuje bieżącą konfigurację i zapisuje ją na nowo\n"

#: src/pam-config.c:88
msgid "  -q, --query       Query for installed modules and options\n"
msgstr "  -q, --query       Wyszukuje zainstalowane moduły i opcje\n"

#: src/pam-config.c:90
msgid "      --list-modules  List all supported modules\n"
msgstr ""

#: src/pam-config.c:92
msgid "      --help        Give this help list\n"
msgstr "  --help         Wyświetla tę pomoc\n"

#: src/pam-config.c:93
msgid "  -u, --usage       Give a short usage message\n"
msgstr "  --usage        Pokazuje krótką wiadomość dot. używania\n"

#: src/pam-config.c:94
msgid "  -v, --version     Print program version\n"
msgstr "  --version      Wyświetla wersję programu\n"

