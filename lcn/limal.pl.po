#: src/ByteBuffer.cpp:249
#: src/ByteBuffer.cpp:299
#: src/ByteBuffer.cpp:316
msgid "ByteBuffer index out of bounds: size=%1, pos=%2."
msgstr "Indeks ByteBuffer poza zakresem: rozmiar=%1, pozycja=%2"

#: src/LDAPUrlImpl.cpp:107
msgid "Unable to parse LDAP URL query string '%1'."
msgstr "Przetworzenie zapytania URL LDAP '%1' nie powiodło się."

#: src/LDAPUrlImpl.cpp:148
msgid "Invalid LDAP URL query parameter '%1'."
msgstr "Nieprawidłowy parametr zapytania URL LDAP '%1'."

#: src/Logger.cpp:312
msgid "The specified maximum log file size is too big."
msgstr "Podany maksymalny rozmiar pliku dziennika jest zbyt duży."

#: src/Logger.cpp:318
msgid "The log file path should be set."
msgstr "Ścieżka pliku dziennika powinna być ustawiona."

#: src/Logger.cpp:324
msgid "The specified log file name '%1' is not absolute"
msgstr "Podana nazwa pliku dziennika nie jest bezwzględna: '%1'"

#: src/PathName.cpp:193
msgid "The specified filename component '%1' contains a filename separator"
msgstr "Podany komponent nazwy '%1' zawiera separator nazwy"

#: src/PathName.cpp:355
msgid "Invalid drive letter prefix in the specified path list."
msgstr "Nieprawidłowa litera dysku w podanej liście ścieżek."

#: src/PathName.cpp:363
msgid "The first pathname list component has to be empty or contain a drive letter prefix"
msgstr "Pierwszy komponent listy ścieżek musi być pusty lub zawierać literę dysku"

#: src/PathName.cpp:377
msgid "The path list contains an element with a filename separator."
msgstr "Lista ścieżek zawiera element z separatorem nazwy pliku."

#: src/UrlBase.cpp:159
msgid "The %1 url component is not allowed in this scheme"
msgstr "Komponent %1 URL nie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:178
msgid "Invalid %1 URL component data '%2'."
msgstr "Nieprawidłowe dane komponentu URL (%1): '%2'."

#: src/UrlBase.cpp:185
msgid "Invalid data in the %1 URL component."
msgstr "Nieprawidłowe dane w komponencie URL (%1)."

#: src/UrlBase.cpp:270
msgid "The authority url component is not allowed in this scheme"
msgstr "Komponent nadrzędny URL nie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:277
msgid "The query string url component is not allowed in this scheme"
msgstr "Komponent zapytania adresu URL nie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:284
msgid "The fragment string url component is not allowed in this scheme"
msgstr "Komponent fragmentu łańcucha adresu URL nie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:780
#: src/UrlBase.cpp:1193
msgid "Path parameter parsing is not supported for this URL."
msgstr "Przetwarzanie parametrów ścieżki nie jest obsługiwane dla tego adresu URL."

#: src/UrlBase.cpp:829
#: src/UrlBase.cpp:1238
msgid "Query string parsing is not supported for this URL."
msgstr "Przetwarzanie tekstu zapytania nie jest obsługiwane dla tego adresu URL."

#: src/UrlBase.cpp:864
msgid "The URL scheme is a required component."
msgstr "Schemat URL jest wymaganym komponentem."

#: src/UrlBase.cpp:870
msgid "Invalid URL scheme '%1'."
msgstr "Nieprawidłowy schemat URL '%1'."

#: src/UrlBase.cpp:970
msgid "The username component is not allowed in this scheme"
msgstr "Komponent nazwy użytkownika nie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:1004
msgid "The password component is not allowed in this scheme"
msgstr "Komponent hasła nie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:1034
msgid "The URL scheme requires a host."
msgstr "Schemat URL wymaga hosta."

#: src/UrlBase.cpp:1044
msgid "The hostname component is not allowed in this scheme"
msgstr "Komponent nazwy hosta nie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:1109
msgid "The port number component is not allowed in this scheme"
msgstr "Komponent numeru portu n ie jest dozwolony w tym schemacie"

#: src/UrlBase.cpp:1130
msgid "The URL scheme requires a path."
msgstr "Schemat URL wymaga ścieżki."

#: src/UrlBase.cpp:1369
msgid "The IPvFuture URL host format is not supported."
msgstr "Format IPvFuture adresu URL hosta nie jest obsługiwany."

#: src/UrlBase.cpp:1403
msgid "Invalid hostname URL component data '%1'."
msgstr "Nieprawidłowe dane w adresie hosta URL '%1'."

#: src/UrlBase.cpp:1433
msgid "Invalid port number URL component data '%1'."
msgstr "Nieprawidłowe dane w numerze portu URL '%1'."

#: src/UrlBase.cpp:1459
#: src/UrlBase.cpp:1472
msgid "A relative path is not allowed if authority exists."
msgstr "Względna ścieżka nie jest dozwolona, jeśli usługa istnieje."

#: src/Url.cpp:64
#: src/Url.cpp:85
msgid "The URL implementation reference cannot be empty."
msgstr "Referencja do implementacji URL nie może być pusta."

#: src/Url.cpp:123
#: src/Url.cpp:130
msgid "Unable to parse url string '%1'."
msgstr "Przetworzenie łańcucha '%1' w adresie URL nie powiodło się."

#: src/UrlUtils.cpp:199
msgid "The URL-encoded string may not contain a NULL byte."
msgstr "Zakodowany łańcuch URL nie może zawierać bitu NULL."

#: src/UrlUtils.cpp:260
#: src/UrlUtils.cpp:298
msgid "Invalid split separator character."
msgstr "Nieprawidłowy znak separatora."

#: src/UrlUtils.cpp:372
msgid "Invalid parameter map join separator character."
msgstr "Nieprawidłowy znak podziału przy łączeniu map parametrów."

#: src/UrlUtils.cpp:448
msgid "Unable to parse main URL components."
msgstr "Nie można przetworzyć głównych komponentów adresu URL."

#: src/UrlUtils.cpp:493
msgid "Unable to parse URL authority components."
msgstr "Nie można przetworzyć nadrzędnych komponentów adresu URL."

#: src/ValueCheck.cpp:129
#: src/ValueCheck.cpp:160
#: src/ValueCheck.cpp:241
msgid "The value check may not contain a NULL pointer."
msgstr "Test wartości nie może zawierać wskaźnika zerowego (NULL)."

#: typemaps/perl5/_limal_Callback.i:242
msgid "Perl callback subroutine died: %1."
msgstr "Podprogram perl callback zakończył działanie: %1."

#: typemaps/perl5/_limal_Callback.i:247
msgid "Perl callback subroutine returned %1 values?!"
msgstr "Podprogram perl callback zwrócił wartość: %1 ?!"

#: typemaps/perl5/_limal_Callback.i:262
msgid "The response from the Perl callback subroutine could not be understood."
msgstr "Odpowiedź zwrócona przez podprogram Perl callback nie jest zrozumiała."

#: typemaps/perl5/_limal_Callback.i:286
msgid "Invalid perl callback subroutine."
msgstr "Nieprawidłowy podprogram perl callback."

#: typemaps/perl5/_limal_Callback.i:379
msgid "Cannot find swig type information for '%1' request type."
msgstr "Nie można znaleźć informacji o typie swig dla typu żądania dla '%1'."

#: typemaps/perl5/_limal_Callback.i:386
msgid "Cannot allocate Perl object for '%1' request type."
msgstr "Nie można przydzielić obiektu Perl dla typu żądania '%1'."

#: typemaps/perl5/_limal_Callback.i:409
msgid "Cannot find swig type information for '%1' result type."
msgstr "Nie można znaleźć informacji o typie swig dla typu wynikowego dla '%1'."

