#: ../../nsswitch/pam_winbind.c:533
msgid "Success"
msgstr "Sukces"

#: ../../nsswitch/pam_winbind.c:535
msgid "No primary Domain Controler available"
msgstr "Brak kontolera podstawowej domeny"

#: ../../nsswitch/pam_winbind.c:537
msgid "No domain controllers found"
msgstr "Nie znaleziono kontrolerów domen"

#: ../../nsswitch/pam_winbind.c:539
msgid "No logon servers"
msgstr "Nie znaleziono serwerów logowania"

#: ../../nsswitch/pam_winbind.c:541
msgid "Password too short"
msgstr "Hasło zbyt krótkie"

#: ../../nsswitch/pam_winbind.c:543
msgid "The password of this user is too recent to change"
msgstr "Hasło dla tego użytkownika jest również wymagane do zmiany"

#: ../../nsswitch/pam_winbind.c:545
msgid "Password is already in password history"
msgstr "Hasło znajduje sie aktualnie w historii haseł."

#: ../../nsswitch/pam_winbind.c:547
msgid "Your password has expired"
msgstr "Hasło wygasło"

#: ../../nsswitch/pam_winbind.c:549
msgid "You need to change your password now"
msgstr "Należy zmienieć teraz hasło"

#: ../../nsswitch/pam_winbind.c:551
msgid "You are not allowed to logon from this workstation"
msgstr "Nie ma praw do logowania się z tej stacji roboczej"

#: ../../nsswitch/pam_winbind.c:553
msgid "You are not allowed to logon at this time"
msgstr "Nie ma praw do logowania sie w tej chwili"

#: ../../nsswitch/pam_winbind.c:555
msgid "Your account has expired. Please contact your System administrator"
msgstr "Konto wygasło. Proszę skontaktować się z administratorem systemu."

#: ../../nsswitch/pam_winbind.c:558
msgid "Your account is disabled. Please contact your System administrator"
msgstr "Konto jest wyłączone. Prosze się skontaktować z administratorem systemu"

#: ../../nsswitch/pam_winbind.c:561
msgid "Your account has been locked. Please contact your System administrator"
msgstr "Konto jest zablokowane. Proszę skontaktować się z administratorem systemu"

#: ../../nsswitch/pam_winbind.c:564 ../../nsswitch/pam_winbind.c:566
#: ../../nsswitch/pam_winbind.c:568
msgid "Invalid Trust Account"
msgstr "Niepoprawne zaufane konto"

#: ../../nsswitch/pam_winbind.c:570
msgid "Access is denied"
msgstr "Brak dostępu"

#: ../../nsswitch/pam_winbind.c:845
msgid "Your password expires today"
msgstr "Hasło wygaśnie jutro."

#: ../../nsswitch/pam_winbind.c:851
#, c-format
msgid "Your password will expire in %d %s"
msgstr "Hasło wygaśnie za %d %s."

#: ../../nsswitch/pam_winbind.c:852
msgid "days"
msgstr "dni"

#: ../../nsswitch/pam_winbind.c:852
msgid "day"
msgstr "dzień"

#: ../../nsswitch/pam_winbind.c:1193
msgid "Grace login. Please change your password as soon you're online again"
msgstr "Proszę zmienić swoje hasło przy następnym logowaniu."

#: ../../nsswitch/pam_winbind.c:1203
msgid "Domain Controller unreachable, using cached credentials instead. Network resources may be unavailable"
msgstr "Kontroler domeny jest nieosiągalny, zamiast tego użyte są zapisane wcześniej dane. Zasoby sieciowe mogą być niedostępne."

#: ../../nsswitch/pam_winbind.c:1228
msgid ""
"Failed to establish your Kerberos Ticket cache due time differences\n"
"with the domain controller.  Please verify the system time.\n"
msgstr ""

#: ../../nsswitch/pam_winbind.c:1258
#, c-format
msgid "Your password "
msgstr "Hasło"

#: ../../nsswitch/pam_winbind.c:1265
#, c-format
msgid "must be at least %d characters; "
msgstr "musi mieć co najmniej %d znaków,"

#: ../../nsswitch/pam_winbind.c:1275
#, c-format
msgid "cannot repeat any of your previous %d passwords; "
msgstr "nie można powtarzać żadnego z poprzednich %d haseł, "

#: ../../nsswitch/pam_winbind.c:1287
#, c-format
msgid "must contain capitals, numerals or punctuation; and cannot contain your account or full name; "
msgstr "musi zawierać litery, cyfry lub inne znaki, i nie może zawierać nazwy konta"

#: ../../nsswitch/pam_winbind.c:1298
#, c-format
msgid "Please type a different password. Type a password which meets these requirements in both text boxes."
msgstr "Proszę podać inne hasło, które spełnia wymagania w polach tekstowych."

#: ../../nsswitch/pam_winbind.c:1600
msgid "Password does not meet complexity requirements"
msgstr "Hasła nie są zgodne."

#: ../../nsswitch/pam_winbind.c:2114
msgid "Password: "
msgstr "Hasło: "

