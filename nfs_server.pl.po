#. not fatal - write other dirs.
#: src/NfsServer.ycp:165
#, ycp-format
msgid ""
"Unable to create a missing directory:\n"
"%1"
msgstr ""
"Nie można utworzyć brakującego katalogu\n"
"'%1'"

#. error popup message
#: src/NfsServer.ycp:174
msgid ""
"Unable to write to /etc/exports.\n"
"No changes will be made to the\n"
"exported directories.\n"
msgstr ""
"Nie można zapisać pliku /etc/exports.\n"
"Nie zostaną dokonane żadne zmiany \n"
"w eksportowanych katalogach.\n"

#. dialog label
#: src/NfsServer.ycp:194
msgid "Writing NFS Server Configuration"
msgstr "Zapisywanie konfiguracji serwera NFS"

#. progress stage label
#: src/NfsServer.ycp:196
msgid "Save /etc/exports"
msgstr "Zapisywanie pliku /etc/exports"

#. progress stage label
#: src/NfsServer.ycp:198
msgid "Restart services"
msgstr "Restartowanie usług"

#. progress step label
#: src/NfsServer.ycp:201
msgid "Saving /etc/exports..."
msgstr "Zapisywanie pliku /etc/exports..."

#. progress step label
#: src/NfsServer.ycp:203
msgid "Restarting services..."
msgstr "Restartowanie usług..."

#. final progress step label
#: src/NfsServer.ycp:205
msgid "Finished"
msgstr "Zakończone"

#. help text
#: src/NfsServer.ycp:212
msgid "Writing NFS server settings. Please wait..."
msgstr "Zapisywanie ustawień serwera NFS. Proszę czekać..."

#. error popup message
#: src/NfsServer.ycp:286
msgid ""
"Unable to restart the NFS server.\n"
"Your changes will be active after reboot.\n"
msgstr ""
"Nie można zrestartować serwera NFS.\n"
"Twoje zmiany zostaną aktywowane po restarcie systemu.\n"

#. summary header; directories exported by NFS
#: src/NfsServer.ycp:311
msgid "NFS Exports"
msgstr "Eksporty NFS"

#. summary text
#: src/nfs_server.ycp:107
msgid "NFS server is enabled"
msgstr "Serwer NFS włączony"

#. summary text
#: src/nfs_server.ycp:110
msgid "NFS server is disabled"
msgstr "Serwer NFS wyłączony"

#. error message
#: src/nfs_server.ycp:127
#, ycp-format
msgid "Required packages (%1) are not installed."
msgstr "Nie są zainstalowane wymagane pakiety (%1)."

#. error
#: src/nfs_server.ycp:177
#: src/nfs_server.ycp:215
msgid "No mount point specified."
msgstr "Niepodane miejsce montowania."

#. error popup message
#: src/nfs_server.ycp:183
#: src/ui.ycp:89
msgid ""
"The exports table already\n"
"contains this directory."
msgstr ""
"Tabela eksportów zawiera\n"
"już ten katalog."

#. Command line help text for the nfs-server module
#: src/nfs_server.ycp:235
msgid "Configuration of NFS server"
msgstr "Konfiguracja serwera NFS"

#. command line action help
#: src/nfs_server.ycp:243
msgid "NFS server configuration summary"
msgstr "Zestawienie konfiguracji serwera NFS"

#. command line action help
#: src/nfs_server.ycp:248
msgid "Start NFS server"
msgstr "Uruchom serwer NFS"

#. command line action help
#: src/nfs_server.ycp:253
msgid "Stop NFS server"
msgstr "Zatrzymaj serwera NFS"

#. command line action help
#: src/nfs_server.ycp:258
msgid "Add a directory to export"
msgstr "Dodaj katalog do wyeksportowania"

#. command line action help
#: src/nfs_server.ycp:263
msgid "Delete a directory from export"
msgstr "Usuń eksportowany katalog"

#. command line option help
#: src/nfs_server.ycp:270
msgid "Directory to export"
msgstr "Katalog do wyeksportowania"

#. command line option help
#: src/nfs_server.ycp:275
msgid "Host wild card for setting the access to directory"
msgstr "Wzorzec nazwy komputera dla określenia dostępu do katalogu"

#. command line option help (do not transl. 'man exports')
#: src/nfs_server.ycp:280
msgid "Export options (see 'man exports')"
msgstr "Opcje eksportu (zobacz 'man exports')"

#. selection box label
#: src/routines.ycp:96
msgid "Dire&ctories"
msgstr "&Katalogi"

#. error popup message
#: src/routines.ycp:112
#, ycp-format
msgid ""
"The wild card or options string is invalid.\n"
"It must be shorter than 70 characters and it\n"
"must not contain spaces.\n"
msgstr ""
"Wzorzec albo opcja niepoprawna.\n"
"Musi być ona krótsza niż 70 znaków \n"
"i nie może zawierać spacji.\n"

#. error popup message
#: src/routines.ycp:130
msgid ""
"Invalid option.\n"
"Only letters, digits, and the characters =/.,_- are allowed."
msgstr ""
"Niepoprawna opcja.\n"
"Dozwolone są tylko litery, cyfry i znaki =/.,_- ."

#. error popup message
#: src/routines.ycp:162
#, ycp-format
msgid "Unknown option: '%1'"
msgstr "Nieznana opcja: %1"

#. error popup message
#: src/routines.ycp:169
#, ycp-format
msgid "Invalid option: '%1'"
msgstr "Niepoprawna opcja '%1'"

#. text entry label
#: src/ui.ycp:62
msgid "&Directory to Export"
msgstr "Katalog &do wyeksportowania"

#. button label
#: src/ui.ycp:66
msgid "&Browse..."
msgstr "Przeg&lądaj..."

#. message popup; %1, %2 are package names
#: src/ui.ycp:97
#, ycp-format
msgid ""
"The user mode NFS server (%1) cannot export directories\n"
"with spaces in their names.\n"
"Use the kernel-based server (%2) to do that."
msgstr ""
"Serwer NFS trybu użytkownika (%1) nie może eksportować\n"
"katalogów ze spacjami w nazwie.\n"
"Żeby to zrobić musisz zastosować serwer działający na poziomie jądra (%2)."

#. the dir does not exist
#: src/ui.ycp:104
msgid ""
"The directory does not exist.\n"
"Create it?"
msgstr ""
"Katalog nie istnieje.\n"
"Czy go utworzyć?"

#. title in the file selection dialog
#: src/ui.ycp:117
msgid "Select the Directory to Export"
msgstr "Katalog do wyeksportowania"

#. text entry label
#: src/ui.ycp:170
msgid "&Host Wild Card"
msgstr "&Wzorzec nazwy komputera"

#. text entry label
#: src/ui.ycp:172
msgid "O&ptions"
msgstr "Op&cje"

#. error popup message
#: src/ui.ycp:205
msgid ""
"Options for this wild card\n"
"are already set."
msgstr ""
"Opcje dla tego wzorca\n"
"są już podane."

#. Help, part 1 of 2
#: src/ui.ycp:240
msgid ""
"<P>Here, choose whether to start an NFS server on your computer\n"
"and export some of your directories to others.</P>"
msgstr ""
"<P>Tutaj możesz wybrać czy chcesz uruchomić na swoim komputerze\n"
"serwer NFS i eksportować innym wybrane katalogi.</P>"

#. Help, part 2 of 2
#: src/ui.ycp:245
msgid ""
"<P>If you choose <B>Start NFS Server</B>, clicking <B>Next</B> opens\n"
"a configuration dialog in which to specify the directories to export.</P>"
msgstr ""
"<P>W przypadku wybrania <B>Uruchom serwer NFS</B>, przycisk <B>Dalej</B> otworzy\n"
"okno konfiguracyjne, w którym można podać katalogi do wyeksportowania.</P>"

#. frame label
#: src/ui.ycp:254
msgid "NFS Server"
msgstr "Serwer NFS"

#. radio button label
#: src/ui.ycp:258
msgid "&Start"
msgstr "&Uruchom"

#. radio button label
#: src/ui.ycp:260
msgid "&Do Not Start"
msgstr "Nie uruchamiaj"

#. frame label
#: src/ui.ycp:264
msgid "Firewall"
msgstr "Firewall"

#. dialog title
#: src/ui.ycp:275
msgid "NFS Server Configuration"
msgstr "Konfiguracja serwera NFS"

#. Help, part 1 of 4
#: src/ui.ycp:339
msgid ""
"<P>The upper box contains all the directories to export.\n"
"If a directory is selected, the lower box shows the hosts allowed to\n"
"mount this directory.</P>\n"
msgstr ""
"<P>Górne pole zawiera wszystkie katalogi, które zostaną wyeksportowane.\n"
"Po wybraniu katalogu w dolnym polu zostaną pokazane maszyny, które\n"
"mogą zamontować ten katalog.</P>\n"

#. Help, part 2 of 4
#: src/ui.ycp:345
msgid ""
"<P><b>Host Wild Card</b> sets which hosts can access the selected directory.\n"
"It can be a single host, groups, wild cards, or\n"
"IP networks.</P>\n"
msgstr ""
"<P><b>Wzorzec nazwy komputera</b> określa, które komputery mogą mieć\n"
"dostęp do wybranego katalogu. Może to być pojedyncza maszyna, grupa sieciowa, \n"
"wzorzec albo sieć IP.</P>\n"

#. Help, part 3 of 4, variant for kernel space server
#: src/ui.ycp:354
msgid "<p>Enter an asterisk (<tt>*</tt>) to specify all hosts.</p>"
msgstr "<p>Gwiazdka (<tt>*</tt>) określa wszystkie komputery.</p>"

#. Help, part 3 of 4, variant for user space server
#: src/ui.ycp:359
msgid "<p>Leave the field empty to specify all hosts.</p>"
msgstr "<p>Pozostaw pole puste na oznaczenie wszystkich komputerów.</p>"

#. Help, part 4 of 4
#: src/ui.ycp:363
msgid "<P>Refer to <tt>man exports</tt> for more information.</P>\n"
msgstr "<P>Więcej informacji znajdziesz w podręczniku <tt>man exports</>t.</P>\n"

