msgid "&NFS Servers"
msgstr "Serwery &NFS"

#. selection box label
#: src/ui.ycp:122
msgid "&Exported Directories"
msgstr "Katalogi wy&eksportowane"

#. text entry label
#: src/ui.ycp:173
